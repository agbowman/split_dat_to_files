   CALL print(trim(replace(edw_vendor->qual[d.seq].po_print_format_disp,str_find,str_replace,3))),
   v_bar,
   CALL print(trim(cnvtstring(edw_vendor->qual[d.seq].po_transmit_type_ref,16))),
   v_bar,
   CALL print(trim(replace(edw_vendor->qual[d.seq].po_transmit_type_disp,str_find,str_replace,3))),
   v_bar,
   CALL print(trim(cnvtstring(edw_vendor->qual[d.seq].ship_via_ref,16))), v_bar,
   CALL print(trim(replace(edw_vendor->qual[d.seq].ship_via_disp,str_find,str_replace,3))),
   v_bar,
   CALL print(trim(cnvtstring(edw_vendor->qual[d.seq].vendor_item_lead_time,16))), v_bar,
   CALL print(trim(cnvtstring(edw_vendor->qual[d.seq].vendor_item_lead_time_uom_ref,16))), v_bar,
   CALL print(trim(replace(edw_vendor->qual[d.seq].vendor_item_lead_time_uom_disp,str_find,
     str_replace,3))),
   v_bar,
   CALL print(trim(cnvtstring(edw_vendor->qual[d.seq].price_review_ind,16))), v_bar,
   "3", v_bar, extract_dt_tm_fmt,
   v_bar, row + 1
  WITH noheading, nocounter, format = lfstream,
   maxcol = 1999, maxrow = 1, append
 ;end select
END GO
