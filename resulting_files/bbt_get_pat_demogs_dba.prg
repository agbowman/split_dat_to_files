 SUBROUTINE (populatetransfusionreqind(person_id=f8(value)) =null)
   DECLARE scmv_neg = vc WITH protect, constant("CMV-")
   DECLARE sirradiated = vc WITH protect, constant("IRRADIATED")
   DECLARE sleuko = vc WITH protect, constant("RESLEU")
   SELECT INTO "nl:"
    st_mean = uar_get_code_meaning(trr.special_testing_cd)
    FROM person_trans_req ptr,
     trans_req_r trr,
     bb_isbt_attribute_r biar,
     bb_isbt_attribute bia
    PLAN (ptr
     WHERE ptr.person_id=person_id
      AND ptr.active_ind=1)
     JOIN (trr
     WHERE (trr.requirement_cd= Outerjoin(ptr.requirement_cd))
      AND (trr.active_ind= Outerjoin(1)) )
     JOIN (biar
     WHERE (biar.attribute_cd= Outerjoin(trr.special_testing_cd))
      AND (biar.active_ind= Outerjoin(1)) )
     JOIN (bia
     WHERE (bia.bb_isbt_attribute_id= Outerjoin(biar.bb_isbt_attribute_id))
      AND (bia.active_ind= Outerjoin(1)) )
    ORDER BY trr.special_testing_cd
    HEAD trr.special_testing_cd
     IF (trr.special_testing_cd > 0)
      IF (cnvtupper(substring(1,4,bia.standard_display))=scmv_neg)
       reply->cmv_neg_req_ind = 1
      ELSEIF (cnvtupper(substring(1,10,bia.standard_display))=sirradiated)
       reply->irradiated_req_ind = 1
      ELSEIF (cnvtupper(substring(1,6,bia.standard_display))=sleuko)
       reply->leukoreduced_req_ind = 1
      ENDIF
     ENDIF
    WITH nocounter
   ;end select
   IF (debugind > 0)
    CALL echo("Reply after populating Transfusion Requirement Indicators:")
    CALL echorecord(reply)
   ENDIF
 END ;Subroutine
 SUBROUTINE (populateaborhfields(person_id=f8(value)) =null)
   DECLARE demorhcd = f8 WITH protect, noconstant(0.0)
   DECLARE demoabocd = f8 WITH protect, noconstant(0.0)
   DECLARE demostandardaborhcd = f8 WITH protect, noconstant(0.0)
   DECLARE democontributorcd = f8 WITH protect, noconstant(0.0)
   DECLARE standardaborhcodeset = i4 WITH protect, constant(1640)
   SELECT INTO "nl:"
    FROM person p,
     person_aborh pa
    PLAN (p
     WHERE p.person_id=person_id
      AND p.active_ind=1)
     JOIN (pa
     WHERE p.person_id=pa.person_id
      AND pa.active_ind=1)
    ORDER BY pa.updt_dt_tm
    DETAIL
     demorhcd = pa.rh_cd, demoabocd = pa.abo_cd, democontributorcd = p.contributor_system_cd
    WITH nocounter
   ;end select
   IF (demorhcd=0
    AND demoabocd=0)
    IF (debugind > 0)
     CALL echo(build("Demographic ABO/Rh NOT found."))
    ENDIF
    RETURN(null)
   ENDIF
   SELECT INTO "nl:"
    FROM code_value cv,
     code_value_extension cve,
     code_value_extension cve1
    PLAN (cv
     WHERE cv.code_set=standardaborhcodeset)
     JOIN (cve
     WHERE cve.code_value=cv.code_value
      AND cve.field_name="RhOnly_cd"
      AND cve.field_value=cnvtstring(demorhcd))
     JOIN (cve1
     WHERE cve1.code_value=cv.code_value
      AND cve1.field_name="ABOOnly_cd"
      AND cve1.field_value=cnvtstring(demoabocd))
    DETAIL
     demostandardaborhcd = cv.code_value
    WITH nocounter
   ;end select
   IF (debugind > 0)
    CALL echo(build("Demographic ABO/Rh - demoABO: ",demoabocd," demoRh: ",demorhcd," StandardCd: ",
      demostandardaborhcd," demoContrib: ",democontributorcd))
   ENDIF
   DECLARE aborhcount = i4 WITH protect, noconstant(0)
   FREE RECORD aborhresulthistory
   RECORD aborhresulthistory(
     1 standardaborh[*]
       2 code = f8
   )
   SELECT INTO "nl:"
    FROM person_aborh_result pr
    PLAN (pr
     WHERE pr.person_id=person_id)
    ORDER BY pr.updt_dt_tm DESC
    DETAIL
     aborhcount += 1
     IF (mod(aborhcount,5)=1)
      stat = alterlist(aborhresulthistory->standardaborh,(aborhcount+ 5))
     ENDIF
     aborhresulthistory->standardaborh[aborhcount].code = pr.standard_aborh_cd
    FOOT REPORT
     stat = alterlist(aborhresulthistory->standardaborh,aborhcount)
    WITH nocounter
   ;end select
   IF (debugind > 0)
    CALL echo("ABORh Results:")
    CALL echorecord(aborhresulthistory)
   ENDIF
   DECLARE confirmationfound = i2 WITH protect, noconstant(0)
   IF (size(aborhresulthistory->standardaborh,5)=1)
    IF ((aborhresulthistory->standardaborh[1].code=demostandardaborhcd)
     AND democontributorcd > 0.0
     AND uar_get_code_meaning(democontributorcd)="CERNER")
     SET confirmationfound = 1
    ENDIF
   ELSEIF (size(aborhresulthistory->standardaborh,5) > 1)
    IF ((aborhresulthistory->standardaborh[2].code=aborhresulthistory->standardaborh[1].code)
     AND (aborhresulthistory->standardaborh[1].code=demostandardaborhcd))
     SET confirmationfound = 1
    ENDIF
   ENDIF
   IF (confirmationfound > 0)
    SET reply->abo_cd = demoabocd
    SET reply->rh_cd = demorhcd
   ENDIF
   FREE RECORD aborhresulthistory
 END ;Subroutine
 SUBROUTINE cleanupafterpsv(null)
   FREE RECORD getflexrequest
   FREE RECORD getflexreply
   FREE RECORD orders_rec
 END ;Subroutine
 SUBROUTINE (populatesamplevalidity(order_pref=vc(value),person_id=f8(value),facility_cd=f8(value)) =
  null)
   SET getflexrequest->alert_ind = "N"
   SET v_cust_stat = alterlist(getflexrequest->personlist,1)
   SET getflexrequest->personlist[1].person_id = person_id
   SET getflexrequest->personlist[1].filter_encntr_id = 0.0
   SET getflexrequest->facility_cd = facility_cd
   EXECUTE bbt_get_avail_flex_specs  WITH replace("REQUEST","GETFLEXREQUEST"), replace("REPLY",
    "GETFLEXREPLY")
   IF ((getflexreply->status_data.status="F"))
    IF (debugind > 0)
     CALL echo(build("There was an error with bbt_get_avail_flex_specs."))
    ENDIF
    CALL cleanupafterpsv(null)
    RETURN(null)
   ELSE
    IF (debugind > 0)
     CALL echo("Reply from bbt_get_avail_flex_specs:")
     CALL echorecord(getflexreply)
    ENDIF
   ENDIF
   FREE RECORD orders_rec
   RECORD orders_rec(
     1 cnt = i4
     1 qual[*]
       2 value = f8
   )
   DECLARE tempstr = vc WITH protect, noconstant("")
   DECLARE tempval = f8 WITH protect, noconstant(0.0)
   DECLARE cnt = i4 WITH protect, noconstant(1)
   DECLARE not_found = vc WITH protect, constant("NOTFOUND")
   DECLARE stat = i4 WITH protect, noconstant(0)
   SET tempstr = trim(piece(order_pref,",",cnt,not_found),3)
   WHILE (tempstr != not_found)
     SET tempval = cnvtreal(tempstr)
     IF (tempval > 0)
      SET orders_rec->cnt += 1
      SET stat = alterlist(orders_rec->qual,orders_rec->cnt)
      SET orders_rec->qual[orders_rec->cnt].value = tempval
      SET tempval = - (1)
     ENDIF
     SET cnt += 1
     SET tempstr = trim(piece(order_pref,",",cnt,not_found,3),3)
   ENDWHILE
   IF (debugind > 0)
    CALL echo("Order Preference Parsed:")
    CALL echorecord(orders_rec)
   ENDIF
   DECLARE catcnt = i4 WITH protect, noconstant(0)
   DECLARE speccnt = i4 WITH protect, noconstant(0)
   DECLARE ordcnt = i4 WITH protect, noconstant(0)
   SET speccnt = size(getflexreply->personlist[1].specimen,5)
   SET catcnt = size(orders_rec->qual,5)
   FOR (i = 1 TO speccnt)
    SET ordcnt = size(getflexreply->personlist[1].specimen[i].orders,5)
    FOR (j = 1 TO ordcnt)
      FOR (k = 1 TO catcnt)
        IF ((orders_rec->qual[k].value=getflexreply->personlist[1].specimen[i].orders[j].catalog_cd))
         IF ((reply->sample_status_valid_dt_tm < getflexreply->personlist[1].specimen[i].expire_dt_tm
         ))
          SET reply->sample_status_valid_dt_tm = getflexreply->personlist[1].specimen[i].expire_dt_tm
          SET reply->sample_status_valid_ind = 1
          SET reply->sample_number = getflexreply->personlist[1].specimen[i].accession
         ENDIF
        ENDIF
      ENDFOR
    ENDFOR
   ENDFOR
   CALL cleanupafterpsv(null)
 END ;Subroutine
#exit_script
END GO
