  public
  DECLARE step_stat_edreview = f8 WITH constant(uar_get_code_by("MEANING",cs_step_stat,"EDREVIEW")),
  public
  DECLARE cs_edreview_stat = i4 WITH constant(4002463), public
  DECLARE edreview_stat_available = f8 WITH constant(uar_get_code_by("MEANING",cs_edreview_stat,
    "AVAILABLE")), public
  DECLARE edreview_stat_agreed = f8 WITH constant(uar_get_code_by("MEANING",cs_edreview_stat,"AGREED"
    )), public
  DECLARE edreview_stat_disagreed = f8 WITH constant(uar_get_code_by("MEANING",cs_edreview_stat,
    "DISAGREED")), public
  DECLARE edreview_stat_acknowledged = f8 WITH constant(uar_get_code_by("MEANING",cs_edreview_stat,
    "ACKNOWLEDGED")), public
  DECLARE edreview_stat_removed = f8 WITH constant(null), public
 ENDIF
 DECLARE studyinstanceuid = vc WITH protect
 DECLARE suidtextlength = i4 WITH protect, noconstant(0)
 DECLARE msg4100933 = i4 WITH protect, constant(4100933)
 DECLARE hmsg4100933 = i4 WITH protect, noconstant(0)
 SET hmsg4100933 = uar_srvselectmessage(msg4100933)
 SET hreq = uar_srvcreaterequest(hmsg4100933)
 IF (validate(reply)=0)
  RECORD reply(
    1 status_data
      2 status = c1
      2 subeventstatus[*]
        3 operationname = c25
        3 operationstatus = c1
        3 targetobjectname = c25
        3 targetobjectvalue = vc
  )
 ENDIF
 SELECT INTO "nl:"
  FROM im_study_parent_r imsp,
   im_study ims
  WHERE (imsp.parent_entity_id=request->procedure_id)
   AND imsp.parent_entity_name="CV_PROC"
   AND imsp.im_study_id=ims.im_study_id
  DETAIL
   studyinstanceuid = ims.study_uid
  WITH nocounter
 ;end select
 SET suidtextlength = textlen(trim(studyinstanceuid))
 IF (suidtextlength > 0)
  SET stat = uar_srvsetstring(hreq,"siuid",nullterm(studyinstanceuid))
  SET stat = uar_srvsetdouble(hreq,"procId",request->procedure_id)
  SET hrep = uar_srvcreatereply(hmsg4100933)
  SET iret = uar_srvexecute(hmsg4100933,hreq,hrep)
  SET htransstatus = uar_srvgetstruct(hrep,"transactionStatus")
  IF (uar_srvgetshort(htransstatus,"successIndicator")=1)
   SET reply->status_data.status = "S"
  ELSE
   SET reply->status_data.status = "F"
   CALL cv_log_stat(cv_error,"SERVER CALL","F","CV_MATCH_UPDATE_DICOM_DATA",
    "Call to  Discrete Data Server to update Normalcy Failed")
  ENDIF
 ELSE
  SET reply->status_data.status = "F"
  CALL cv_log_stat(cv_error,"VALIDATE","F","CV_MATCH_UPDATE_DICOM_DATA","StudyInstanceUID is Empty")
 ENDIF
 IF ((reply->status_data.status != "S"))
  SET reqinfo->commit_ind = 0
  CALL echorecord(request)
  CALL echorecord(reply)
 ELSE
  SET reqinfo->commit_ind = 1
 ENDIF
 CALL cv_log_msg_post("MOD 00 03/17/17 JT023123")
END GO
