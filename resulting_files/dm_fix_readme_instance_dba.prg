CREATE PROGRAM dm_fix_readme_instance:dba
 IF ( NOT (validate(readme_data,0)))
  FREE SET readme_data
  RECORD readme_data(
    1 ocd = i4
    1 readme_id = f8
    1 instance = i4
    1 readme_type = vc
    1 description = vc
    1 script = vc
    1 check_script = vc
    1 data_file = vc
    1 par_file = vc
    1 blocks = i4
    1 log_rowid = vc
    1 status = vc
    1 message = c255
    1 options = vc
    1 driver = vc
    1 batch_dt_tm = dq8
  )
 ENDIF
 SET readmes_2_upd = 0
 SET envi_id = 0.0
 SET readme_data->message = "Starting readme"
 SET readme_data->status = "F"
 SELECT INTO "NL:"
  FROM dm_info di
  WHERE di.info_domain="DATA MANAGEMENT"
   AND di.info_name="INHOUSE DOMAIN"
  WITH nocounter
 ;end select
 IF (curqual > 0)
  SET readme_data->message = "Auto success for in house domains."
  SET readme_data->status = "S"
  GO TO exit_script
 ENDIF
 SELECT INTO "NL:"
  dm.info_number
  FROM dm_info dm
  WHERE dm.info_domain="DATA MANAGEMENT"
   AND dm.info_name="DM_ENV_ID"
  DETAIL
   envi_id = dm.info_number
  WITH nocounter
 ;end select
