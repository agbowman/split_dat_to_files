 SUBROUTINE add_group_item(parent_cd,child_cd,code_set)
   SET grp_total_cnt = (grp_total_cnt+ 1)
   SET stat = alterlist(internal->groups,grp_total_cnt)
   SET internal->groups[grp_total_cnt].parent_cd = parent_cd
   SET internal->groups[grp_total_cnt].child_cd = child_cd
   SET internal->groups[grp_total_cnt].code_set = code_set
   SET internal->groups[grp_total_cnt].preexist_flag = 0
   RETURN(0)
 END ;Subroutine
 SUBROUTINE check_groups(abc)
   SELECT INTO "nl:"
    cvg.parent_cd
    FROM code_value_group cvg,
     (dummyt d  WITH seq = value(grp_total_cnt))
    PLAN (d)
     JOIN (cvg
     WHERE (cvg.parent_code_value=internal->groups[d.seq].parent_cd)
      AND (cvg.child_code_value=internal->groups[d.seq].child_cd)
      AND (cvg.code_set=internal->groups[d.seq].code_set))
    HEAD REPORT
     grp_preexist_cnt = 0
    DETAIL
     grp_preexist_cnt = (grp_preexist_cnt+ 1), internal->groups[d.seq].preexist_flag = 1
    WITH nocounter
   ;end select
 END ;Subroutine
 SUBROUTINE insert_groups(abc)
  INSERT  FROM code_value_group cvg,
    (dummyt d  WITH seq = value(grp_total_cnt))
   SET cvg.parent_code_value = internal->groups[d.seq].parent_cd, cvg.child_code_value = internal->
    groups[d.seq].child_cd, cvg.code_set = internal->groups[d.seq].code_set,
    cvg.collation_seq = d.seq, cvg.updt_id = 99, cvg.updt_task = 99,
    cvg.updt_applctx = 99, cvg.updt_cnt = 0, cvg.updt_dt_tm = cnvtdatetime(curdate,curtime)
   PLAN (d
    WHERE (internal->groups[d.seq].preexist_flag=0))
    JOIN (cvg
    WHERE (internal->groups[d.seq].parent_cd > 0)
     AND (internal->groups[d.seq].child_cd > 0))
   WITH nocounter
  ;end insert
  SET grp_add_cnt = curqual
 END ;Subroutine
 SUBROUTINE meaning_code(mc_codeset,mc_meaning)
   SET mc_code = 0.0
   SET mc_text = fillstring(12," ")
   SET mc_text = mc_meaning
   SELECT INTO "nl:"
    FROM code_value cv
    WHERE cv.code_set=mc_codeset
     AND cv.cdf_meaning=mc_text
    DETAIL
     mc_code = cv.code_value
    WITH maxqual(cv,1), nocounter
   ;end select
   IF (mc_code > 0.0)
    RETURN(mc_code)
   ELSE
    RETURN(- (1.0))
   ENDIF
 END ;Subroutine
