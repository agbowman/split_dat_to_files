 RECORD internal(
   1 value[*]
     2 code_value = f8
     2 display = c40
     2 description = c60
     2 cdf_meaning = c12
     2 definition = c100
     2 value_found_ind = i2
   1 groups[*]
     2 parent_cd = f8
     2 child_cd = f8
     2 code_set = i4
     2 preexist_flag = i2
 )
