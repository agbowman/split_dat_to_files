 SUBROUTINE getbloodnetproducts(null)
   DECLARE req1202004 = i4 WITH protect, constant(1202004)
   DECLARE hmsg1202004 = i4 WITH protect, noconstant(0)
   DECLARE hreq1202004 = i4 WITH protect, noconstant(0)
   DECLARE hrep1202004 = i4 WITH protect, noconstant(0)
   DECLARE hstatus_data = i4 WITH protect, noconstant(0)
   DECLARE replystatus = vc WITH protect, noconstant("")
   SET hmsg1202004 = uar_srvselectmessage(req1202004)
   IF (hmsg1202004=0)
    CALL populate_subeventstatus("hMsg1202004","F","set hMsg1202004","hMsg1202004 returns 0")
    GO TO exitgetbloodnetproducts
   ENDIF
   SET hreq1202004 = uar_srvcreaterequest(hmsg1202004)
   IF (hreq1202004=0)
    CALL populate_subeventstatus("hReq1202004","F","set hReq1202004","hReq1202004 returns 0")
    GO TO exitgetbloodnetproducts
   ENDIF
   SET hrep1202004 = uar_srvcreatereply(hmsg1202004)
   IF (hrep1202004=0)
    CALL populate_subeventstatus("hRep1202004","F","set hRep1202004","hRep1202004 returns 0")
    GO TO exitgetbloodnetproducts
   ENDIF
   SET stat = uar_srvsetdouble(hreq1202004,"destination_inv_area_cd",request->cur_inv_area_cd)
   SET stat = uar_srvexecute(hmsg1202004,hreq1202004,hrep1202004)
   SET hstatus_data = uar_srvgetstruct(hrep1202004,"status_data")
   CALL uar_srvgetstring(hstatus_data,"status",replystatus,uar_srvgetstringlen(hstatus_data,"status")
    )
   IF (debugind=1)
    CALL echo("Request/ Reply in cer_log, file name: 1202004REQUEST.dat/1202004REPLY.dat")
    CALL uar_crmlogmessage(hreq1202004,"1202004REQUEST.dat")
    CALL uar_crmlogmessage(hrep1202004,"1202004REPLY.dat")
   ENDIF
   IF (((replystatus="S") OR (replystatus="P")) )
    IF (uar_srvgetitemcount(hrep1202004,"receive_list") > 0)
     CALL fsireplyasreqtopathnet(hrep1202004)
    ELSE
     SET reply->status_data.status = replystatus
