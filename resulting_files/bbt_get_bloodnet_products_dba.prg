   ENDIF
#exitgetbloodnetproducts
   CALL uar_srvdestroyinstance(hreq1202004)
   CALL uar_srvdestroyinstance(hmsg1202004)
   CALL uar_srvdestroyinstance(hrep1202004)
 END ;Subroutine
 SUBROUTINE (fsireplyasreqtopathnet(hrep1202004=i4(ref)) =null)
   DECLARE req225011 = i4 WITH protect, constant(225011)
   DECLARE hmsg225011 = i4 WITH protect, noconstant(0)
   DECLARE hreq225011 = i4 WITH protect, constant(hrep1202004)
   DECLARE hrep225011 = i4 WITH protect, noconstant(0)
   DECLARE hstatus_data = i4 WITH protect, noconstant(0)
   DECLARE replystatus = vc WITH protect, noconstant("F")
   DECLARE admincount = i4 WITH protect, noconstant(0)
   DECLARE hadmin = i4 WITH protect, noconstant(0)
   SET hmsg225011 = uar_srvselectmessage(req225011)
   IF (hmsg225011=0)
    CALL populate_subeventstatus("hMsg225011","F","set hMsg225011","hMsg225011 returns 0")
    GO TO exitfsireplyasreqtopathnet
   ENDIF
   SET admincount = uar_srvgetitemcount(hreq225011,"receive_list")
   FOR (i = 0 TO (admincount - 1))
     SET hadmin = uar_srvgetitem(hreq225011,"receive_list",i)
     SET stat = uar_srvsetdouble(hadmin,"destination_loc_cd",ownerareacd)
     SET stat = uar_srvsetdouble(hadmin,"destination_inv_area_cd",request->cur_inv_area_cd)
