   SET hrep225011 = uar_srvcreatereply(hmsg225011)
   IF (hrep225011=0)
    CALL populate_subeventstatus("hRep225011","F","set hRep225011","hRep225011 returns 0")
    GO TO exitfsireplyasreqtopathnet
   ENDIF
   SET stat = uar_srvexecute(hmsg225011,hreq225011,hrep225011)
