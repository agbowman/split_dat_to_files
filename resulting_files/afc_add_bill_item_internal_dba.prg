 SUBROUTINE add_bill_item(add_begin,add_end)
   FOR (x = add_begin TO add_end)
     DECLARE code_set = i4
     DECLARE cdf_meaning = c12
     DECLARE active_code = f8
     SET code_set = 48
     SET cdf_meaning = "ACTIVE"
     DECLARE codecnt = i4
     SET codecnt = 1
     IF ((request->bill_item[x].active_status_cd=0))
      SET stat = uar_get_meaning_by_codeset(code_set,cdf_meaning,codecnt,active_code)
     ENDIF
     SET new_nbr = 0.0
     SELECT INTO "nl:"
      y = seq(bill_item_seq,nextval)"##################;rp0"
      FROM dual
      DETAIL
       new_nbr = cnvtreal(y)
      WITH format, counter
     ;end select
     IF (curqual=0)
      SET failed = gen_nbr_error
      RETURN
     ELSE
      SET request->bill_item[x].bill_item_id = new_nbr
     ENDIF
     INSERT  FROM bill_item b
      SET b.bill_item_id = new_nbr, b.ext_parent_reference_id =
       IF ((request->bill_item[x].ext_parent_reference_id=0)) new_nbr
       ELSE request->bill_item[x].ext_parent_reference_id
       ENDIF
       , b.ext_parent_contributor_cd =
       IF ((request->bill_item[x].ext_parent_contributor_cd=0)) 0
       ELSE request->bill_item[x].ext_parent_contributor_cd
       ENDIF
       ,
       b.ext_child_reference_id =
       IF ((request->bill_item[x].ext_child_reference_id=0)) 0
       ELSE request->bill_item[x].ext_child_reference_id
       ENDIF
       , b.ext_child_contributor_cd =
       IF ((request->bill_item[x].ext_child_contributor_cd=0)) 0
       ELSE request->bill_item[x].ext_child_contributor_cd
       ENDIF
       , b.ext_description = request->bill_item[x].ext_description,
       b.ext_owner_cd =
       IF ((request->bill_item[x].ext_owner_cd=0)) 0
       ELSE request->bill_item[x].ext_owner_cd
       ENDIF
       , b.parent_qual_cd =
       IF ((request->bill_item[x].parent_qual_cd=0)) 0
       ELSE request->bill_item[x].parent_qual_cd
       ENDIF
       , b.charge_point_cd =
       IF ((request->bill_item[x].charge_point_cd=0)) 0
       ELSE request->bill_item[x].charge_point_cd
       ENDIF
       ,
       b.physician_qual_cd =
       IF ((request->bill_item[x].physician_qual_cd=0)) 0
       ELSE request->bill_item[x].physician_qual_cd
       ENDIF
       , b.calc_type_cd =
       IF ((request->bill_item[x].calc_type_cd=0)) 0
       ELSE request->bill_item[x].calc_type_cd
       ENDIF
       , b.beg_effective_dt_tm =
       IF ((request->bill_item[x].beg_effective_dt_tm <= 0)) cnvtdatetime(sysdate)
       ELSE cnvtdatetime(request->bill_item[x].beg_effective_dt_tm)
       ENDIF
       ,
       b.end_effective_dt_tm =
       IF ((request->bill_item[x].end_effective_dt_tm <= 0)) cnvtdatetime("31-DEC-2100 23:59:59")
       ELSE cnvtdatetime(request->bill_item[x].end_effective_dt_tm)
       ENDIF
       , b.active_ind =
       IF ((request->bill_item[x].active_ind_ind=false)) true
       ELSE request->bill_item[x].active_ind
       ENDIF
       , b.active_status_cd =
       IF ((request->bill_item[x].active_status_cd=0)) active_code
       ELSE request->bill_item[x].active_status_cd
       ENDIF
       ,
       b.active_status_prsnl_id =
       IF ((request->bill_item[x].active_status_prsnl_id=0)) reqinfo->updt_id
       ELSE request->bill_item[x].active_status_prsnl_id
       ENDIF
       , b.active_status_dt_tm =
       IF ((request->bill_item[x].active_status_dt_tm <= 0)) cnvtdatetime(sysdate)
       ELSE cnvtdatetime(request->bill_item[x].active_status_dt_tm)
       ENDIF
       , b.updt_cnt = 0,
       b.updt_dt_tm = cnvtdatetime(sysdate), b.updt_id = reqinfo->updt_id, b.updt_applctx = reqinfo->
       updt_applctx,
       b.updt_task = reqinfo->updt_task, b.ext_parent_entity_name = "BILL_ITEM", b
       .ext_child_entity_name = "",
       b.logical_domain_id =
       IF ((validate(request->logical_domain_id,- (0.00001)) != - (0.00001))) validate(request->
         logical_domain_id,- (0.00001))
       ELSE 0.0
       ENDIF
       , b.logical_domain_enabled_ind =
       IF ((validate(request->logical_domain_enabled_ind,- (1)) != - (1))) validate(request->
         logical_domain_enabled_ind,- (1))
       ELSE 0
       ENDIF
      WITH nocounter
     ;end insert
     IF (curqual=0)
      SET failed = insert_error
      RETURN
     ELSE
      SET reply->bill_item[x].bill_item_id = request->bill_item[x].bill_item_id
     ENDIF
   ENDFOR
 END ;Subroutine
#end_program
