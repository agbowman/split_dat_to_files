 IF (sbr_dfsm_initialize(null)=0)
  GO TO end_program
 ENDIF
 IF ((dm_err->debug_flag > 0))
  CALL echorecord(dfsm_seq_lst_rec)
 ENDIF
 SET v_batchsize = sbr_dfsm_check_batchsize(null)
 IF (v_batchsize=0)
  GO TO end_program
 ENDIF
 IF ((dfsm_seq_lst_rec->seq_input=v_wildcard))
  IF (sbr_dfsm_wildcard_maint(null)=0)
   GO TO end_program
  ENDIF
 ENDIF
 SET stat = alterlist(auto_ver_request->qual,1)
 SET auto_ver_request->qual[1].rdds_event = "Creating Sequence Match Row"
 SET auto_ver_request->qual[1].cur_environment_id = dfsm_seq_lst_rec->target
 SET auto_ver_request->qual[1].paired_environment_id = dfsm_seq_lst_rec->source
 SET auto_ver_request->qual[1].event_reason = dm_get_seq_val
 SET stat = alterlist(auto_ver_request->qual[1].detail_qual,dfsm_seq_lst_rec->seq_cnt)
 FOR (v_gs_cnt = 1 TO dfsm_seq_lst_rec->seq_cnt)
   SET dm_err->eproc = concat("Processing sequence ",dfsm_seq_lst_rec->seq_lst[v_gs_cnt].seq_name,
    "...",trim(cnvtstring(v_gs_cnt))," out of ",
    trim(cnvtstring(dfsm_seq_lst_rec->seq_cnt)))
   CALL disp_msg(" ",dm_err->logfile,0)
   SET v_insertstatus = sbr_dfsm_insert_seq(v_gs_cnt)
   IF (v_insertstatus=1)
    SET dm_err->eproc = concat("Inserted data and processing table(s) for sequence ",dfsm_seq_lst_rec
     ->seq_lst[v_gs_cnt].seq_name,".")
    CALL disp_msg(" ",dm_err->logfile,0)
    IF (sbr_dfsm_load_table_names(v_gs_cnt)=1)
     SET dfsm_seq_lst_rec->seq_lst[v_gs_cnt].info_number = dfsm_seq_lst_rec->seq_lst[v_gs_cnt].
     tgt_src_min_val
     FOR (v_ts_cnt = 1 TO dfsm_seq_lst_rec->seq_lst[v_gs_cnt].tbl_cnt)
       IF ((dm_err->debug_flag > 0))
        SET dm_err->eproc = concat("MEM USAGE FROM MAIN AT BEGINING OF EXECUTE C1 ",cnvtstring(curmem
          ))
        CALL disp_msg(" ",dm_err->logfile,0)
       ENDIF
       IF ((dfsm_seq_lst_rec->seq_lst[v_gs_cnt].tgt_last_num != - (1))
        AND (dfsm_seq_lst_rec->seq_lst[v_gs_cnt].src_last_num != - (1)))
        SET dm_err->eproc = concat("Processing table: ",dfsm_seq_lst_rec->seq_lst[v_gs_cnt].tbl_lst[
         v_ts_cnt].tbl_name," ",dfsm_seq_lst_rec->seq_lst[v_gs_cnt].tbl_lst[v_ts_cnt].tbl_col,
         "... table ",
         trim(cnvtstring(v_ts_cnt))," out of ",trim(cnvtstring(dfsm_seq_lst_rec->seq_lst[v_gs_cnt].
           tbl_cnt)))
        CALL disp_msg(" ",dm_err->logfile,0)
        EXECUTE dm2_find_sequence_match_c1 v_gs_cnt, v_ts_cnt
       ENDIF
       IF ((dm_err->debug_flag > 0))
        SET dm_err->eproc = concat("MEM USAGE FROM MAIN AT RETURN FROM C1 ",cnvtstring(curmem))
        CALL disp_msg(" ",dm_err->logfile,0)
       ENDIF
       IF ((dm_err->err_ind=1))
        GO TO end_program
       ENDIF
     ENDFOR
     IF (sbr_dfsm_update_tbl(v_gs_cnt)=0)
      GO TO end_program
     ENDIF
    ELSE
     GO TO end_program
    ENDIF
   ELSEIF (v_insertstatus=2)
