  IF ((dfsm_seq_lst_rec->seq_input=char(42)))
   SET dm_err->eproc = "Validating if DM_REFCHG_INVALID_XLAT backfill has already occurred."
   CALL disp_msg(" ",dm_err->logfile,0)
   SET dfs_pl_name = concat("@MERGE",trim(cnvtstring(dfsm_seq_lst_rec->source)),trim(cnvtstring(
      dfsm_seq_lst_rec->target)))
   SELECT INTO "NL:"
    FROM dm_info di
    WHERE di.info_domain="INVALID XLAT BACKFILL COMPLETE"
     AND di.info_name=dfs_pl_name
    WITH nocounter
   ;end select
   IF (check_error(dm_err->eproc)=1)
    CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
    SET dm_err->err_ind = 1
    GO TO end_program
   ENDIF
   IF (curqual=0)
    INSERT  FROM dm_info di
     SET di.info_domain = "INVALID XLAT BACKFILL COMPLETE", di.info_name = dfs_pl_name, di.updt_dt_tm
       = cnvtdatetime(curdate,curtime3)
     WITH nocounter
    ;end insert
    COMMIT
   ENDIF
  ENDIF
 ENDIF
 CALL final_disp_msg(logfile_prefix)
 FREE RECORD dfsm_seq_lst_rec
 FREE RECORD rdds_special_tbl
 SUBROUTINE sbr_dfsm_initialize(null)
   DECLARE input_seq = vc WITH protect
   DECLARE input_source = f8 WITH protect
   DECLARE rtn_status = i2 WITH protect
   DECLARE sdi_env_status = vc WITH protect, noconstant("N/A")
   DECLARE sdi_env_flag = i2 WITH protect, noconstant(1)
   SET input_seq = build(cnvtupper(trim( $1)))
   SET input_source =  $2
   SET rtn_status = - (1)
   IF (dm2_rdds_get_tgt_id(dm2_ref_data_doc)=0)
    SET rtn_status = 0
   ELSE
    SET dm_err->eproc = "Beginning Sequence Match Process"
    CALL disp_msg(" ",dm_err->logfile,0)
    IF (input_source=0)
     SET dfsm_seq_lst_rec->target = 0
    ELSE
     SET dfsm_seq_lst_rec->target = dm2_ref_data_doc->env_target_id
    ENDIF
    IF (rtn_status != 0)
     SET dfsm_seq_lst_rec->source = input_source
     SET dfsm_seq_lst_rec->db_link = concat("MERGE",trim(cnvtstring(dfsm_seq_lst_rec->source)),trim(
       cnvtstring(dfsm_seq_lst_rec->target)))
     SET dfsm_seq_lst_rec->info_domain = concat("MERGE",trim(cnvtstring(dfsm_seq_lst_rec->source)),
      trim(cnvtstring(dm2_ref_data_doc->mock_target_id)),v_info_domain_suffix)
     SET dfsm_seq_lst_rec->info_date = cnvtdatetime(curdate,curtime3)
     SET dfsm_seq_lst_rec->seq_input = input_seq
     SET dm_err->eproc = "Checking environment setup..."
     CALL disp_msg(" ",dm_err->logfile,0)
     IF ((dfsm_seq_lst_rec->source != 0))
      SET sdi_env_status = dm2_rdds_chk_rel_val_rows(dfsm_seq_lst_rec->source,dfsm_seq_lst_rec->
       target,concat("@",dfsm_seq_lst_rec->db_link))
      IF ((dm_err->err_ind=1))
       SET rtn_status = 0
       SET sdi_env_flag = - (1)
      ELSEIF (sdi_env_status != "BOTH_FOUND")
       SET sdi_env_flag = 0
      ENDIF
     ENDIF
     IF (sdi_env_flag=0)
      SET rtn_status = 0
      SET dm_err->err_ind = 1
      IF ((dfsm_seq_lst_rec->seq_input=v_wildcard))
       SET dm_err->eproc = concat("Source/Target relationship has not been validated. ",
        "Please perform sequence matching via dm_merge_domain_adm ",
        "so that relationship validation will occur. In CCL type: ","dm_merge_domain_adm go.")
       CALL disp_msg(" ",dm_err->logfile,0)
      ELSE
       SET dm_err->eproc = concat("Source/Target relationship has not been validated. ",
        "Sequence matching for a single sequence cannot be performed.")
       CALL disp_msg(" ",dm_err->logfile,0)
      ENDIF
     ELSEIF (sdi_env_flag=1)
      SET dm_err->eproc = concat("Source: ",trim(cnvtstring(dfsm_seq_lst_rec->source))," Target: ",
       trim(cnvtstring(dfsm_seq_lst_rec->target)))
      CALL disp_msg(" ",dm_err->logfile,0)
      SET get_date = format(cnvtdatetime(dfsm_seq_lst_rec->info_date),"mm/dd/yyyy hh:mm:ss;;d")
      SET dm_err->eproc = concat("Domain: ",trim(dfsm_seq_lst_rec->info_domain),
       "  /  Input Parameter : ",dfsm_seq_lst_rec->seq_input,"  /  Date: ",
       get_date)
      CALL disp_msg(" ",dm_err->logfile,0)
      IF (sbr_dfsm_chk_par(input_seq,input_source)=1)
       SET rtn_status = 1
      ELSE
       SET rtn_status = 0
      ENDIF
     ENDIF
    ENDIF
   ENDIF
   IF (rtn_status=1
    AND fsm_mover_called=0)
    SET dm_err->eproc = "refresh target's matadata if a refresh is needed"
    CALL disp_msg(" ",dm_err->logfile,0)
    EXECUTE dm2_rdds_metadata_refresh "NONE"
    IF ((dm_err->err_ind=1))
     SET rtn_status = 0
    ENDIF
    IF (rtn_status=1)
     SET dm_err->eproc = "refresh source's matadata if a refresh is needed"
     CALL disp_msg(" ",dm_err->logfile,0)
     EXECUTE dm2_rdds_metadata_refresh build("@",dfsm_seq_lst_rec->db_link)
     IF ((dm_err->err_ind=1))
      SET rtn_status = 0
     ENDIF
    ENDIF
   ENDIF
   RETURN(rtn_status)
 END ;Subroutine
 SUBROUTINE sbr_dfsm_chk_par(sdcp_seq,sdcp_source)
   DECLARE sdcp_rtn_status = i2 WITH protect
   IF (sdcp_seq=v_wildcard)
    SET sdcp_rtn_status = 1
    IF ((dm2_ref_data_doc->mock_target_id != dm2_ref_data_doc->env_target_id))
     SET sdcp_rtn_status = 0
    ELSE
     IF (sdcp_source != 0)
      IF (load_sequence_names(null)=0)
       SET sdcp_rtn_status = 0
      ENDIF
     ENDIF
    ENDIF
   ELSE
    IF (load_sequence_names(null)=0)
     SET sdcp_rtn_status = 0
    ELSE
     SET sdcp_rtn_status = 1
    ENDIF
   ENDIF
   RETURN(sdcp_rtn_status)
 END ;Subroutine
 SUBROUTINE sbr_dfsm_wildcard_maint(null)
   DECLARE adwm_rtn_status = i2 WITH protect
   SET adwm_rtn_status = 0
   IF ((dfsm_seq_lst_rec->source=0))
    SET dm_err->eproc = "Inserting default rows into dm_info because source parameter is 0."
    CALL disp_msg(" ",dm_err->logfile,0)
    INSERT  FROM dm_info
     (info_domain, info_name, info_number,
     info_date, updt_task)(SELECT
      dfsm_seq_lst_rec->info_domain, d.sequence_name, d.last_number,
      cnvtdatetime(curdate,curtime3), v_dfsm_updt_task
      FROM dm2_user_sequences d)
    ;end insert
    SET dm_err->eproc = "Failure in inserting row into dm_info where source = 0"
    IF (check_error(dm_err->eproc)=1)
     CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
     SET adwm_rtn_status = 0
    ELSE
     COMMIT
     SET adwm_rtn_status = 1
    ENDIF
   ELSE
    COMMIT
    SET adwm_rtn_status = 1
   ENDIF
   RETURN(adwm_rtn_status)
 END ;Subroutine
 SUBROUTINE load_sequence_names(null)
   DECLARE lsn_source_link = vc WITH protect
   DECLARE lsn_src_cnt = i4 WITH protect, noconstant(0)
   DECLARE lsn_tgt_cnt = i4 WITH protect, noconstant(0)
   DECLARE lsn_pos = i4 WITH protect, noconstant(0)
   DECLARE lsn_num = i4 WITH protect, noconstant(0)
   DECLARE lsn_flag = i2 WITH protect, noconstant(0)
   DECLARE lsn_idx = i4 WITH protect, noconstant(0)
   DECLARE lsn_dm_info_source = vc WITH protect
   SET lsn_source_link = concat("dm2_user_sequences@",dfsm_seq_lst_rec->db_link)
   SET lsn_dm_info_source = concat("dm_info@",trim(dfsm_seq_lst_rec->db_link))
   SET dm_err->eproc =
   "Inserting sequence name(s) that exist in the source but not in current domain"
   CALL disp_msg(" ",dm_err->logfile,0)
   INSERT  FROM dm_info di
    (di.info_domain, di.info_name, di.info_number,
    di.updt_dt_tm, di.updt_cnt)(SELECT
     dfsm_seq_lst_rec->info_domain, di2.info_name, di2.info_number,
     sysdate, 0
     FROM (parser(lsn_dm_info_source) di2)
     WHERE di2.info_domain=concat("MERGE",trim(cnvtstring(dm2_ref_data_doc->mock_target_id)),trim(
       cnvtstring(dfsm_seq_lst_rec->source)),"SEQMATCH")
      AND di2.info_name=patstring(dfsm_seq_lst_rec->seq_input)
      AND  NOT (di2.info_name IN (
     (SELECT
      di3.info_name
      FROM dm_info di3
      WHERE (di3.info_domain=dfsm_seq_lst_rec->info_domain)))))
    WITH nocounter
   ;end insert
   IF (check_error(dm_err->eproc)=1)
    CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
    RETURN(0)
   ELSE
    COMMIT
   ENDIF
   CALL echo("**********************************************************")
   CALL echo("**                                                      **")
   CALL echo(concat(trim(cnvtstring(curqual))," sequence/s were inserted from source"))
   CALL echo("**                                                      **")
   CALL echo("**********************************************************")
   SET dm_err->eproc = "Loading sequence name(s)"
   CALL disp_msg(" ",dm_err->logfile,0)
   SELECT INTO "nl:"
    dmussrc.sequence_name, dmussrc.last_number
    FROM (parser(lsn_source_link) dmussrc)
    WHERE dmussrc.sequence_name=patstring(dfsm_seq_lst_rec->seq_input)
     AND  NOT (dmussrc.sequence_name IN (
    (SELECT
     di3.info_name
     FROM dm_info di3
     WHERE (di3.info_domain=dfsm_seq_lst_rec->info_domain))))
    ORDER BY dmussrc.sequence_name
    HEAD REPORT
     stat = alterlist(dfsm_seq_lst_rec->seq_lst,(dfsm_seq_lst_rec->seq_cnt+ 10)), lsn_src_cnt = 0
    DETAIL
     lsn_src_cnt = (lsn_src_cnt+ 1), dfsm_seq_lst_rec->seq_cnt = (dfsm_seq_lst_rec->seq_cnt+ 1)
     IF (mod(lsn_src_cnt,10)=1
      AND lsn_src_cnt != 1)
      stat = alterlist(dfsm_seq_lst_rec->seq_lst,(dfsm_seq_lst_rec->seq_cnt+ 9))
     ENDIF
     dfsm_seq_lst_rec->seq_lst[dfsm_seq_lst_rec->seq_cnt].seq_name = dmussrc.sequence_name,
     dfsm_seq_lst_rec->seq_lst[dfsm_seq_lst_rec->seq_cnt].tgt_last_num = - (1), dfsm_seq_lst_rec->
     seq_lst[dfsm_seq_lst_rec->seq_cnt].src_last_num = dmussrc.last_number
    FOOT REPORT
     stat = alterlist(dfsm_seq_lst_rec->seq_lst,dfsm_seq_lst_rec->seq_cnt)
    WITH nocounter
   ;end select
   IF (check_error(dm_err->eproc)=1)
    CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
    RETURN(0)
   ENDIF
   SELECT INTO "nl:"
    dmus.sequence_name, dmus.last_number
    FROM dm2_user_sequences dmus
    WHERE dmus.sequence_name=patstring(dfsm_seq_lst_rec->seq_input)
     AND  NOT (dmus.sequence_name IN (
    (SELECT
     di3.info_name
     FROM dm_info di3
     WHERE (di3.info_domain=dfsm_seq_lst_rec->info_domain))))
    ORDER BY dmus.sequence_name
    HEAD REPORT
     stat = alterlist(dfsm_seq_lst_rec->seq_lst,(dfsm_seq_lst_rec->seq_cnt+ 10)), lsn_tgt_cnt = 0
    DETAIL
     IF (locateval(lsn_num,1,size(dfsm_seq_lst_rec->seq_lst,5),dmus.sequence_name,dfsm_seq_lst_rec->
      seq_lst[lsn_num].seq_name) > 0)
      dfsm_seq_lst_rec->seq_lst[lsn_num].tgt_last_num = dmus.last_number
     ELSE
      lsn_tgt_cnt = (lsn_tgt_cnt+ 1), dfsm_seq_lst_rec->seq_cnt = (dfsm_seq_lst_rec->seq_cnt+ 1)
      IF (mod(lsn_tgt_cnt,10)=1
       AND lsn_tgt_cnt != 1)
       stat = alterlist(dfsm_seq_lst_rec->seq_lst,(dfsm_seq_lst_rec->seq_cnt+ 9))
      ENDIF
      dfsm_seq_lst_rec->seq_lst[dfsm_seq_lst_rec->seq_cnt].seq_name = dmus.sequence_name,
      dfsm_seq_lst_rec->seq_lst[dfsm_seq_lst_rec->seq_cnt].tgt_last_num = dmus.last_number,
      dfsm_seq_lst_rec->seq_lst[dfsm_seq_lst_rec->seq_cnt].src_last_num = - (1)
     ENDIF
    FOOT REPORT
     stat = alterlist(dfsm_seq_lst_rec->seq_lst,dfsm_seq_lst_rec->seq_cnt)
    WITH nocounter
   ;end select
   IF (check_error(dm_err->eproc)=1)
    CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
    RETURN(0)
   ENDIF
   IF ((dfsm_seq_lst_rec->seq_cnt=0))
    CALL echo("**********************************************************")
    CALL echo("**                                                      **")
    CALL echo("No more sequences need to be calculated")
    CALL echo("**                                                      **")
    CALL echo("**********************************************************")
    RETURN(1)
   ELSE
    CALL echo("**********************************************************")
    CALL echo("**                                                      **")
    CALL echo(concat(trim(cnvtstring(dfsm_seq_lst_rec->seq_cnt))," sequence/s will be calculated"))
    CALL echo("**                                                      **")
    CALL echo("**********************************************************")
   ENDIF
   SELECT INTO "nl:"
    FROM dm_info
    WHERE info_domain="SEQUENCE MATCH CONFIG"
     AND info_name="USE RDDS SEQ MATCH"
    WITH nocounter
   ;end select
   IF (check_error(dm_err->eproc)=1)
    CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
    RETURN(0)
   ENDIF
   IF (curqual != 0)
    SET dm_err->eproc = "Getting max and min values"
    SELECT INTO "nl:"
     t3.info_domain, t3.info_number
     FROM dm_info t3
     WHERE t3.info_domain=patstring(concat("RDDS SEQ MATCH:",dfsm_seq_lst_rec->seq_input))
      AND list(t3.info_domain,t3.info_name) IN (
     (SELECT
      t1.info_domain, max(t1.info_name)
      FROM dm_info t1,
       (value(build("DM_INFO@",dfsm_seq_lst_rec->db_link,"  t2")))
      WHERE t1.info_domain=patstring(concat("RDDS SEQ MATCH:",dfsm_seq_lst_rec->seq_input))
       AND t1.info_domain=t2.info_domain
       AND t1.info_number=t2.info_number
       AND t1.info_name=t2.info_name
       AND (list(t1.info_name) <
      (SELECT
       t6.info_name
       FROM dm_info t6
       WHERE list(t6.info_domain,t6.info_name) IN (
       (SELECT
        t4.info_domain, min(t4.info_name)
        FROM dm_info t4,
         (value(build("DM_INFO@",dfsm_seq_lst_rec->db_link," t5")))
        WHERE t4.info_domain=patstring(concat("RDDS SEQ MATCH:",dfsm_seq_lst_rec->seq_input))
         AND t4.info_domain=t5.info_domain
         AND t4.info_number != t5.info_number
         AND t4.info_name=t5.info_name
        GROUP BY t4.info_domain))
        AND t6.info_domain=t3.info_domain))
      GROUP BY t1.info_domain))
     DETAIL
      lsn_idx = 0, lsn_idx = locateval(lsn_idx,1,dfsm_seq_lst_rec->seq_cnt,substring(16,(size(t3
         .info_domain) - 16),t3.info_domain),dfsm_seq_lst_rec->seq_lst[lsn_idx].seq_name)
      IF (lsn_idx > 0)
       dfsm_seq_lst_rec->seq_lst[lsn_idx].tgt_src_min_val = t3.info_number
      ENDIF
     WITH nocounter
    ;end select
    IF (curqual=0)
     SELECT INTO "nl:"
      temp = count(*), t1.info_domain
      FROM dm_info t1,
       (value(build("DM_INFO@",dfsm_seq_lst_rec->db_link)) t2)
      WHERE t1.info_domain=patstring(concat("RDDS SEQ MATCH:",dfsm_seq_lst_rec->seq_input))
       AND t1.info_domain=t2.info_domain
       AND t1.info_number=t2.info_number
       AND t1.info_name=t2.info_name
      GROUP BY t1.info_domain
      DETAIL
       lsn_idx = 0, lsn_idx = locateval(lsn_idx,1,dfsm_seq_lst_rec->seq_cnt,substring(16,(size(t1
          .info_domain) - 16),t1.info_domain),dfsm_seq_lst_rec->seq_lst[lsn_idx].seq_name)
       IF (lsn_idx > 0)
        dfsm_seq_lst_rec->seq_lst[lsn_idx].temp_cnt = temp
       ENDIF
      WITH nocounter
     ;end select
     SELECT INTO "nl:"
      temp2 = count(*)
      FROM dm_info t1
      WHERE t1.info_domain=patstring(concat("RDDS SEQ MATCH:",dfsm_seq_lst_rec->seq_input))
      GROUP BY t1.info_domain
      DETAIL
       lsn_idx = 0, lsn_idx = locateval(lsn_idx,1,dfsm_seq_lst_rec->seq_cnt,substring(16,(size(t1
          .info_domain) - 16),t1.info_domain),dfsm_seq_lst_rec->seq_lst[lsn_idx].seq_name)
       IF (lsn_idx > 0)
        IF ((dfsm_seq_lst_rec->seq_lst[lsn_idx].temp_cnt=temp2))
         dfsm_seq_lst_rec->seq_lst[lsn_idx].temp_cnt = - (1)
        ELSE
         dfsm_seq_lst_rec->seq_lst[lsn_idx].temp_cnt = 1
        ENDIF
       ENDIF
      WITH nocounter
     ;end select
     FOR (lp_cnt = 1 TO dfsm_seq_lst_rec->seq_cnt)
      IF ((dm_err->debug_flag=1))
       CALL echo(build("temp_cnt =",dfsm_seq_lst_rec->seq_lst[lp_cnt].temp_cnt))
      ENDIF
      IF ((dfsm_seq_lst_rec->seq_lst[lp_cnt].temp_cnt=- (1)))
       SELECT INTO "nl:"
        temp1 = max(t1.info_number)
        FROM dm_info t1
        WHERE t1.info_domain=patstring(concat("RDDS SEQ MATCH:",dfsm_seq_lst_rec->seq_lst[lp_cnt].
          seq_name))
        DETAIL
         dfsm_seq_lst_rec->seq_lst[lp_cnt].tgt_src_min_val = temp1
        WITH nocounter
       ;end select
      ENDIF
     ENDFOR
    ENDIF
    SELECT INTO "nl:"
     t3.info_domain, t3.info_number
     FROM dm_info t3
     WHERE t3.info_domain=patstring(concat("RDDS SEQ MATCH:",dfsm_seq_lst_rec->seq_input))
      AND list(t3.info_domain,t3.info_name) IN (
     (SELECT
      t1.info_domain, min(t1.info_name)
      FROM dm_info t1,
       (value(build("DM_INFO@",dfsm_seq_lst_rec->db_link,"  t2")))
      WHERE t1.info_domain=patstring(concat("RDDS SEQ MATCH:",dfsm_seq_lst_rec->seq_input))
       AND t1.info_domain=t2.info_domain
       AND t1.info_number != t2.info_number
       AND t1.info_name=t2.info_name
      GROUP BY t1.info_domain))
     DETAIL
      lsn_idx = 0, lsn_idx = locateval(lsn_idx,1,dfsm_seq_lst_rec->seq_cnt,substring(16,(size(t3
         .info_domain) - 16),t3.info_domain),dfsm_seq_lst_rec->seq_lst[lsn_idx].seq_name)
      IF (lsn_idx > 0)
       dfsm_seq_lst_rec->seq_lst[lsn_idx].tgt_src_max_val = t3.info_number
      ENDIF
     WITH nocounter
    ;end select
    IF (check_error(dm_err->eproc) != 0)
     CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
     RETURN(0)
    ENDIF
   ENDIF
   IF ((((dfsm_seq_lst_rec->seq_lst[dfsm_seq_lst_rec->seq_cnt].tgt_last_num=- (1))) OR ((
   dfsm_seq_lst_rec->seq_lst[dfsm_seq_lst_rec->seq_cnt].src_last_num=- (1))))
    AND (dfsm_seq_lst_rec->seq_input != v_wildcard))
    SET dm_err->emsg = concat("Input ",dfsm_seq_lst_rec->seq_input,
     " not found in both Source and Target.")
    SET dm_err->err_ind = 1
    SET dm_err->eproc = "LOAD_SEQUENCE_NAMES"
    CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
    RETURN(0)
   ENDIF
   RETURN(1)
 END ;Subroutine
 SUBROUTINE sbr_dfsm_insert_seq(insert_seq_cnt)
   DECLARE v_is_cnt = i4 WITH protect
   DECLARE v_result = i2 WITH protect
   DECLARE v_num_loops = i4 WITH protect
   DECLARE v_ora_error = i2 WITH protect
   SET v_is_cnt = - (1)
   SET v_result = 0
   SET v_num_loops = 1
   SET v_ora_error = 1
   WHILE (v_ora_error=1
    AND v_num_loops <= 2)
     SET v_ora_error = 0
     SELECT INTO "NL:"
      cnt = count(di.info_name)
      FROM dm_info di
      WHERE (di.info_name=dfsm_seq_lst_rec->seq_lst[insert_seq_cnt].seq_name)
       AND (di.info_domain=dfsm_seq_lst_rec->info_domain)
       AND (di.info_number > - (1))
      DETAIL
       v_is_cnt = cnt
      WITH nocounter
     ;end select
     SET dm_err->eproc = "Selecting rows for insert validation."
     IF (check_error(dm_err->eproc)=1)
      CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
      SET v_result = 0
     ELSE
      IF (v_is_cnt)
       SET v_result = 2
      ELSE
       SET v_result = 1
       INSERT  FROM dm_info
        SET info_domain = dfsm_seq_lst_rec->info_domain, info_name = dfsm_seq_lst_rec->seq_lst[
         insert_seq_cnt].seq_name, info_number = - (1),
         updt_task = v_dfsm_updt_task
        WITH nocounter
       ;end insert
       SET dm_err->eproc = "Inserting sequence into dm_info row."
       IF (check_error(dm_err->eproc)=1)
        IF (findstring(v_constrainterr,dm_err->emsg) > 0)
         IF (v_num_loops=1)
          SET dm_err->err_ind = 0
         ELSE
          SET dm_err->err_ind = 1
          CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
         ENDIF
         SET v_ora_error = 1
         SET v_result = 0
        ELSE
         CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
         SET v_result = 0
        ENDIF
       ENDIF
      ENDIF
     ENDIF
     SET v_num_loops = (1+ v_num_loops)
   ENDWHILE
   RETURN(v_result)
 END ;Subroutine
 SUBROUTINE sbr_dfsm_load_table_names(lsn_cnt)
   DECLARE tn_source_link = vc WITH protect
   DECLARE tn_tbl_in_list = vc WITH protect, noconstant("")
   DECLARE tn_tbl_idx = i4 WITH protect, noconstant(0)
   SET tn_source_link = concat("dm2_user_tab_columns@",dfsm_seq_lst_rec->db_link)
   SET tn_tbl_in_list = concat("(","dtd.reference_ind = 1 or dtd.table_name in",rdds_special_tbl->
    tbl_list,")")
   SELECT INTO "nl:"
    dcd.table_name, dcd.column_name
    FROM dm_rdds_tbl_doc dtd,
     dm_rdds_col_doc dcd
    WHERE dcd.root_entity_name=dcd.table_name
     AND (dcd.sequence_name=dfsm_seq_lst_rec->seq_lst[lsn_cnt].seq_name)
     AND dcd.table_name=dtd.table_name
     AND dcd.column_name=dcd.root_entity_attr
     AND parser(tn_tbl_in_list)
     AND  EXISTS (
    (SELECT
     "x"
     FROM (parser(concat("dm_rdds_col_doc@",dfsm_seq_lst_rec->db_link)) sdcd)
     WHERE sdcd.root_entity_name=dcd.root_entity_name
      AND sdcd.sequence_name=dcd.sequence_name
      AND sdcd.table_name=dcd.table_name
      AND sdcd.column_name=dcd.column_name
      AND sdcd.root_entity_attr=dcd.root_entity_attr))
     AND  EXISTS (
    (SELECT
     "x"
     FROM dm2_user_tab_columns uttgt
     WHERE uttgt.table_name=dtd.table_name
      AND uttgt.column_name=dcd.column_name))
     AND  EXISTS (
    (SELECT
     "x"
     FROM (parser(tn_source_link) utsrc)
     WHERE utsrc.table_name=dtd.table_name
      AND utsrc.column_name=dcd.column_name))
     AND  NOT ( EXISTS (
    (SELECT
     "x"
     FROM dm_info ditgt
     WHERE ditgt.info_domain="RDDS SKIP SEQMATCH"
      AND ditgt.info_name=dtd.table_name)))
    HEAD REPORT
     stat = alterlist(dfsm_seq_lst_rec->seq_lst[lsn_cnt].tbl_lst,10), dfsm_seq_lst_rec->seq_lst[
     lsn_cnt].tbl_cnt = 0
    DETAIL
     dfsm_seq_lst_rec->seq_lst[lsn_cnt].tbl_cnt = (dfsm_seq_lst_rec->seq_lst[lsn_cnt].tbl_cnt+ 1)
     IF (mod(dfsm_seq_lst_rec->seq_lst[lsn_cnt].tbl_cnt,10)=1
      AND (dfsm_seq_lst_rec->seq_lst[lsn_cnt].tbl_cnt != 1))
      stat = alterlist(dfsm_seq_lst_rec->seq_lst[lsn_cnt].tbl_lst,(dfsm_seq_lst_rec->seq_lst[lsn_cnt]
       .tbl_cnt+ 9))
     ENDIF
     dfsm_seq_lst_rec->seq_lst[lsn_cnt].tbl_lst[dfsm_seq_lst_rec->seq_lst[lsn_cnt].tbl_cnt].tbl_name
      = dcd.table_name, dfsm_seq_lst_rec->seq_lst[lsn_cnt].tbl_lst[dfsm_seq_lst_rec->seq_lst[lsn_cnt]
     .tbl_cnt].tbl_col = dcd.column_name
    FOOT REPORT
     stat = alterlist(dfsm_seq_lst_rec->seq_lst[lsn_cnt].tbl_lst,dfsm_seq_lst_rec->seq_lst[lsn_cnt].
      tbl_cnt)
    WITH nocounter
   ;end select
   SET dm_err->eproc = concat("Retrieving and loading table for sequence ",dfsm_seq_lst_rec->seq_lst[
    lsn_cnt].seq_name)
   IF (check_error(dm_err->eproc)=1)
    CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
    RETURN(0)
   ENDIF
   RETURN(1)
 END ;Subroutine
 SUBROUTINE sbr_dfsm_update_tbl(sdu_cnt)
   SET dm_err->eproc = concat("Update database for - Sequence Name: ",dfsm_seq_lst_rec->seq_lst[
    sdu_cnt].seq_name,"  Number: ",trim(cnvtstring(dfsm_seq_lst_rec->seq_lst[sdu_cnt].info_number)))
   CALL disp_msg(" ",dm_err->logfile,0)
   UPDATE  FROM dm_info i
    SET i.info_number = dfsm_seq_lst_rec->seq_lst[sdu_cnt].info_number, i.info_date = cnvtdatetime(
      curdate,curtime3), i.updt_task = v_dfsm_updt_task
    WHERE (i.info_domain=dfsm_seq_lst_rec->info_domain)
     AND (i.info_name=dfsm_seq_lst_rec->seq_lst[sdu_cnt].seq_name)
    WITH nocounter
   ;end update
   SET dm_err->eproc = "Error in attempting update to dm_info."
   IF (check_error(dm_err->eproc)=1)
    CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
    RETURN(0)
   ENDIF
   COMMIT
   RETURN(1)
 END ;Subroutine
 SUBROUTINE sbr_dfsm_check_batchsize(null)
   DECLARE sdcb_result = f8 WITH protect
   SET sdcb_result = 0
   SELECT INTO "NL:"
    di.info_number
    FROM dm_info di
    WHERE di.info_name="RDDS SEQMATCH BATCHSIZE"
     AND di.info_domain="DATA MANAGEMENT"
    DETAIL
     sdcb_result = di.info_number
    WITH nocounter
   ;end select
   SET dm_err->eproc = "Selecting rows for batch size."
   IF (check_error(dm_err->eproc)=1)
    CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
    SET sdcb_result = 0
   ELSE
    IF (curqual=0)
     INSERT  FROM dm_info
      SET info_domain = "DATA MANAGEMENT", info_name = "RDDS SEQMATCH BATCHSIZE", info_number =
       10000.00,
       updt_task = v_dfsm_updt_task
      WITH nocounter
     ;end insert
     SET dm_err->eproc = "Inserting row into dm_info for batch size."
     IF (check_error(dm_err->eproc)=1)
      CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
      SET sdcb_result = 0
     ELSE
      COMMIT
      SET sdcb_result = 10000
     ENDIF
    ENDIF
   ENDIF
   SET dm_err->eproc = concat("Batch size for match query is set to: ",trim(cnvtstring(sdcb_result)))
   CALL disp_msg(" ",dm_err->logfile,0)
   RETURN(sdcb_result)
 END ;Subroutine
END GO
