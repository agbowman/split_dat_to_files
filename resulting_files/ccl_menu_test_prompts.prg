CREATE PROGRAM ccl_menu_test_prompts
 PROMPT
  "Output device: <MINE> " = mine,
  "123456789012345678901234567890123456789012345678901234567" = curdate,
  "1234567890123456789012345678901234567890123456789012345678" = curdate,
  "12345678901234567890123456789012345678901234567890123456789" = curdate,
  "123456789012345678901234567890123456789012345678901234567890" = curdate,
  "1234567890123456789012345678901234567890123456789012345678901" = curdate,
  "12345678901234567890123456789012345678901234567890123456789012" = curdate,
  "123456789012345678901234567890123456789012345678901234567890123" = curdate,
  "1234567890123456789012345678901234567890123456789012345678901234" = curdate,
  "12345678901234567890123456789012345678901234567890123456789012345" = curdate
 SELECT INTO  $1
  o.person_id, c.display, o.*
  FROM orders o,
   code_value c
  PLAN (o
   WHERE o.person_id=25223799.0)
   JOIN (c
   WHERE c.code_set=200
    AND c.code_value=o.catalog_cd)
  WITH format, separator = " "
 ;end select
END GO
