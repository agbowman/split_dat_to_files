  " ",trim(bed))
 SET code_cnt = 1
 SET admitdoc_cd = 0.0
 SET meaning_admitdoc = fillstring(12," ")
 SET meaning_admitdoc = "ADMITDOC"
 SET stat = uar_get_meaning_by_codeset(333,meaning_admitdoc,code_cnt,admitdoc_cd)
 SELECT INTO "nl:"
  p.seq, epr.seq, p.name_full_formatted
  FROM encntr_prsnl_reltn epr,
   person p
  PLAN (epr
   WHERE admitdoc_cd=epr.encntr_prsnl_r_cd
    AND (request->encntr_id=epr.encntr_id))
   JOIN (p
   WHERE p.person_id=epr.prsnl_person_id)
  DETAIL
   admitdoc = p.name_full_formatted
  WITH nocounter
 ;end select
 SET code_cnt = 1
 SET mrn_cd = 0.0
 SET meaning_mrn = fillstring(12," ")
 SET meaning_mrn = "MRN"
 SET stat = uar_get_meaning_by_codeset(319,meaning_mrn,code_cnt,mrn_cd)
 SELECT INTO "nl:"
  ea.seq, ea.alias
  FROM encntr_alias ea
  WHERE (ea.encntr_id=request->encntr_id)
   AND ea.encntr_alias_type_cd=mrn_cd
   AND ea.active_ind=1
   AND ea.end_effective_dt_tm > cnvtdatetime(sysdate)
  DETAIL
   mednbr = ea.alias
  WITH nocounter
 ;end select
#mainselect
 IF ((request->person_id != 0))
  SELECT INTO "nl:"
   pe.*, pe1.*, ord.*
   FROM product_event pe,
    product_event pe1,
    orders ord
   PLAN (pe
    WHERE (pe.person_id=request->person_id)
     AND pe.event_type_cd=dispense_type_cd)
    JOIN (pe1
    WHERE (pe1.person_id=request->person_id)
     AND pe1.product_id=pe.product_id
     AND pe1.event_type_cd IN (assigned_type_cd, crossmatched_type_cd, autologous_type_cd,
    directed_type_cd))
    JOIN (ord
    WHERE ord.order_id=pe1.order_id)
   DETAIL
    IF (pe1.product_event_id > 0)
     lidx = locateval(lidx1,1,nbr_of_prods,pe.product_event_id,request->productlist[lidx1].
      product_event_id)
     IF (lidx > 0)
      CALL log_message(build("pe1.product_id:",pe1.product_id),log_level_debug)
      IF (check)
       check = 0
       IF (pe1.order_id > 0.0)
        anchor_dt_tm = ord.current_start_dt_tm, related_event_type_cd = pe1.event_type_cd,
        related_encntr_id = pe1.encntr_id,
        order_id = pe1.order_id, product_event_id = - (1)
       ELSE
        anchor_dt_tm = pe1.event_dt_tm, related_event_type_cd = pe1.event_type_cd, related_encntr_id
         = pe1.encntr_id,
        product_event_id = pe1.product_event_id, order_id = - (1)
       ENDIF
      ELSE
       IF (pe1.order_id > 0.0)
        IF (datetimediff(ord.current_start_dt_tm,anchor_dt_tm,4) >= 0)
         anchor_dt_tm = ord.current_start_dt_tm, related_event_type_cd = pe1.event_type_cd,
         related_encntr_id = pe1.encntr_id,
         order_id = pe1.order_id, product_event_id = - (1)
        ENDIF
       ELSE
        IF (datetimediff(pe1.event_dt_tm,anchor_dt_tm,4) >= 0)
         anchor_dt_tm = pe1.event_dt_tm, related_event_type_cd = pe1.event_type_cd, related_encntr_id
          = pe1.encntr_id,
         product_event_id = pe1.product_event_id, order_id = - (1)
        ENDIF
       ENDIF
      ENDIF
     ENDIF
    ENDIF
   WITH nocounter
  ;end select
  IF ((product_event_id=- (1.0))
   AND (order_id=- (1.0)))
   SELECT INTO "nl:"
    pe.*
    FROM product_event pe
    PLAN (pe
     WHERE (pe.person_id=request->person_id)
      AND pe.event_type_cd=dispense_type_cd
      AND pe.related_product_event_id=0.0
      AND expand(num,1,size(request->productlist,5),pe.product_event_id,request->productlist[num].
      product_event_id))
    DETAIL
     CALL log_message(build("checking in derivatives",pe.product_event_id),log_level_debug),
     anchor_dt_tm = pe.event_dt_tm, related_event_type_cd = pe.event_type_cd,
     related_encntr_id = pe.encntr_id, product_event_id = pe.product_event_id, order_id = - (1)
    WITH nocounter, maxqual(pe,1)
   ;end select
  ENDIF
  IF (hist_demo_ind=1)
   SET current_person_id = request->person_id
   SELECT INTO "nl:"
    pc.from_person_id
    FROM person_combine_det pcd,
     person_combine pc
    PLAN (pcd
     WHERE ((pcd.entity_id=order_id
      AND pcd.entity_name="ORDERS") OR (pcd.entity_id=product_event_id
      AND pcd.entity_name IN ("AUTO_DIRECTED", "ASSIGN", "PATIENT_DISPENSE"))) )
     JOIN (pc
     WHERE pc.person_combine_id=pcd.person_combine_id
      AND pc.active_status_cd=active_status_cd
      AND pc.active_status_dt_tm >= cnvtdatetime(anchor_dt_tm)
      AND pc.active_ind=1)
    ORDER BY pc.active_status_dt_tm
    HEAD REPORT
     current_person_id = pc.from_person_id
    WITH nocounter
   ;end select
   SELECT INTO "nl:"
    pnh.name_full
    FROM person_name_hist pnh
    PLAN (pnh
     WHERE pnh.person_id=current_person_id
      AND pnh.name_type_cd=current_name_type_cd
      AND pnh.transaction_dt_tm <= cnvtdatetime(datetimeadd(anchor_dt_tm,dminute))
      AND  NOT ( EXISTS (
     (SELECT
      pcd.entity_id
      FROM person_combine_det pcd
      WHERE pcd.entity_id=pnh.person_name_hist_id
       AND pcd.entity_name="PERSON_NAME_HIST"
       AND pcd.combine_action_cd=combine_add_cd))))
    ORDER BY pnh.transaction_dt_tm DESC
    HEAD REPORT
     name_full_formatted = pnh.name_full
    WITH nocounter
   ;end select
   SET get_next = 0
   SELECT
    IF (related_event_type_cd=crossmatched_type_cd)
     ORDER BY eah.transaction_dt_tm DESC
    ELSE
     ORDER BY eah.transaction_dt_tm
    ENDIF
    INTO "nl:"
    *
    FROM encntr_alias ea,
     encntr_alias_hist eah
    PLAN (ea
     WHERE ea.encntr_id=related_encntr_id
      AND ea.encntr_alias_type_cd=mrn_type_cd)
     JOIN (eah
     WHERE eah.encntr_alias_id=ea.encntr_alias_id
      AND eah.active_ind=1)
    DETAIL
     IF (related_event_type_cd=crossmatched_type_cd
      AND get_next=0
      AND (datetimediff(anchor_dt_tm,eah.transaction_dt_tm,4) <= - (1)))
      get_next = 1
     ELSEIF (((related_event_type_cd=crossmatched_type_cd
      AND get_next=1
      AND (datetimediff(anchor_dt_tm,eah.transaction_dt_tm,4) >= - (1))) OR (related_event_type_cd
      != crossmatched_type_cd
      AND datetimediff(current_time,eah.transaction_dt_tm,4) > datetimediff(current_time,anchor_dt_tm,
      4))) )
      mednbr = eah.alias, get_next = 0
     ENDIF
    WITH nocounter
   ;end select
  ENDIF
 ENDIF
 SET visinsp = uar_get_code_display(request->visinsp_cd)
 SET reason = uar_get_code_display(request->reason_cd)
 SET locn = uar_get_code_display(request->locn_cd)
 SELECT INTO "n1:"
  FROM product p,
   patient_dispense pd,
   person per_pd,
   (dummyt d  WITH seq = value(nbr_of_prods))
  PLAN (d)
   JOIN (p
   WHERE (request->productlist[d.seq].product_id > 0)
    AND (p.product_id=request->productlist[d.seq].product_id))
   JOIN (pd
   WHERE (request->productlist[d.seq].product_event_id > 0)
    AND (pd.product_event_id=request->productlist[d.seq].product_event_id))
   JOIN (per_pd
   WHERE per_pd.person_id=pd.dispense_prov_id
    AND per_pd.person_id != 0
    AND per_pd.person_id != null)
  ORDER BY per_pd.person_id
  HEAD REPORT
   phys_cnt = 0
  HEAD per_pd.person_id
   phys_cnt += 1, stat = alterlist(physician_name->qual,phys_cnt), physician_name->qual[phys_cnt].
   name = per_pd.name_full_formatted
  WITH nocounter
 ;end select
 EXECUTE cpm_create_file_name_logical "bbt_patient_disp", "txt", "x"
 SELECT INTO cpm_cfn_info->file_name_logical
  p.*, bp.*, pe.*,
  pd.*, d.seq, product_disp = uar_get_code_display(p.product_cd),
  abo_disp = uar_get_code_display(bp.cur_abo_cd), rh_disp = uar_get_code_display(bp.cur_rh_cd), o
  .org_name
  FROM product p,
   blood_product bp,
   product_event pe,
   patient_dispense pd,
   (dummyt d  WITH seq = value(nbr_of_prods)),
   (dummyt d2  WITH seq = 1),
   organization o,
   (dummyt d4  WITH seq = 1)
  PLAN (d)
   JOIN (p
   WHERE (p.product_id=request->productlist[d.seq].product_id)
    AND (request->productlist[d.seq].product_id > 0))
   JOIN (pe
   WHERE (pe.product_event_id=request->productlist[d.seq].product_event_id)
    AND (request->productlist[d.seq].product_event_id > 0))
   JOIN (pd
   WHERE pd.product_event_id=pe.product_event_id)
   JOIN (d4
   WHERE d4.seq=1)
   JOIN (o
   WHERE o.organization_id=p.cur_supplier_id)
   JOIN (d2
   WHERE d2.seq=1)
   JOIN (bp
   WHERE p.product_id=bp.product_id)
  HEAD REPORT
   aborh = "       ", supplier = fillstring(20," "), row 0,
   print_page_head_ind = "Y"
  HEAD PAGE
   IF (print_page_head_ind="Y")
    CALL center(captions->dispense_packing,1,126), col 101, captions->time,
    col 116, curtime"@TIMENOSECONDS;;M", row + 1,
    col 101, captions->as_of_date, col 116,
    curdate"@DATECONDENSED;;d", inc_i18nhandle = 0, inc_h = uar_i18nlocalizationinit(inc_i18nhandle,
     curprog,"",curcclrev),
    row 0
    IF (sub_get_location_name="<<INFORMATION NOT FOUND>>")
     inc_info_not_found = uar_i18ngetmessage(inc_i18nhandle,"inc_information_not_found",
      "<<INFORMATION NOT FOUND>>"), col 1, inc_info_not_found
    ELSE
     col 1, sub_get_location_name
    ENDIF
    row + 1
    IF (sub_get_location_name != "<<INFORMATION NOT FOUND>>")
     IF (sub_get_location_address1 != " ")
      col 1, sub_get_location_address1, row + 1
     ENDIF
     IF (sub_get_location_address2 != " ")
      col 1, sub_get_location_address2, row + 1
     ENDIF
     IF (sub_get_location_address3 != " ")
      col 1, sub_get_location_address3, row + 1
     ENDIF
     IF (sub_get_location_address4 != " ")
      col 1, sub_get_location_address4, row + 1
     ENDIF
     IF (sub_get_location_citystatezip != ",   ")
      col 1, sub_get_location_citystatezip, row + 1
     ENDIF
     IF (sub_get_location_country != " ")
      col 1, sub_get_location_country, row + 1
     ENDIF
    ENDIF
    row + 1
    IF ((request->person_id > 0))
     col 2, captions->name, name_full_formatted
    ELSE
     col 2, captions->name, pd.unknown_patient_text
    ENDIF
    row + 1
    IF (curutc=1)
     temp1 = format(datetimezone(dob,birthtz),"@DATECONDENSED;4;q")
    ELSE
     temp1 = format(dob,"@DATECONDENSED;;d")
    ENDIF
    col 2, captions->birth_dt, temp1,
    col 32, captions->gender, gender,
    row + 1, col 2, captions->patient_abo,
    abo, captions->rh, rh,
    row + 1, col 2, captions->medical_record_no,
    mednbr, row + 1, col 2,
    captions->admit_physician, admitdoc, col 55,
    captions->patient_loc, location, row + 1,
    row + 4, col 2, captions->dispensed,
    col 13, pe.event_dt_tm"@DATETIMECONDENSED;;d", col 32,
    captions->dispensed_by, col 51, request->username,
    col 68, captions->location, col 83,
    locn, row + 1, col 2,
    captions->reason, col 13, reason,
    col 32, captions->visual_inspection, col 51,
    visinsp, col 68, captions->bb_id,
    col 83, request->bbidnbr, row + 1,
    col 2, captions->courier, col 13,
    request->courier, row + 1, col 2.,
    captions->physician, phys_idx = 0
    WHILE (phys_idx < phys_cnt)
      phys_idx += 1, col 16, physician_name->qual[phys_idx].name,
      row + 1
    ENDWHILE
    IF (phys_cnt=0)
     row + 1
    ENDIF
    row + 2, col 8, captions->product_no,
    col 43, captions->expires, col 63,
    captions->product_type, col 85, captions->aborh,
    col 102, captions->supplier, row + 1,
    col 2, "---------------------------------", col 42,
    "--------------", col 59, "--------------------",
    col 85, "------", dash = fillstring(30,"-"),
    col 95, dash, row + 1
   ENDIF
  DETAIL
   IF (row > 55)
    BREAK
   ENDIF
   product = concat(trim(p.product_nbr)," ",trim(p.product_sub_nbr),trim(p.serial_number_txt)),
   prod_nbr_display = concat(trim(bp.supplier_prefix),trim(product)), aborh = concat(trim(abo_disp),
    " ",trim(rh_disp)),
   expiredate = cnvtdatetime(p.cur_expire_dt_tm), product_type = product_disp, col 2,
   prod_nbr_display, col 42, expiredate"@DATETIMECONDENSED;;d",
   col 59, product_type, col 85,
   aborh, supplier = trim(o.org_name), col 95,
   supplier, row + 1
  FOOT PAGE
   row 57, col 1, line,
   row + 1, col 1, captions->report_id,
   col 58, captions->page_no, curpage"###",
   col 97, captions->printed, curdate"@DATECONDENSED;;d",
   col 120, curtime"@TIMENOSECONDS;;M"
  FOOT REPORT
   row 60,
   CALL center(captions->end_of_report,1,126), print_page_head_ind = "N"
  WITH maxcol = 126, maxrow = 61, outerjoin = d2,
   outerjoin(d4), compress, nolandscape
 ;end select
 IF (curqual != 0)
  SET reply->rpt_filename = cpm_cfn_info->file_name
 ENDIF
#exit_script
END GO
