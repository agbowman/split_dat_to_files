  FROM prsnl_org_reltn_type p
  WHERE (p.role_profile=request->role_profile)
   AND p.active_ind=1
   AND p.prsnl_org_reltn_type_cd=type_cd
   AND p.beg_effective_dt_tm <= cnvtdatetime(sysdate)
   AND p.end_effective_dt_tm >= cnvtdatetime(sysdate)
  HEAD REPORT
   index = 0
  DETAIL
   index += 1
   IF (mod(index,10)=1)
    stat = alterlist(reply->prsnl_org_reltn_type_qual,(index+ 9))
   ENDIF
   reply->prsnl_org_reltn_type_qual[index].prsnl_org_reltn_type_id = p.prsnl_org_reltn_type_id
  FOOT REPORT
   stat = alterlist(reply->prsnl_org_reltn_type_qual,index)
  WITH nocounter
 ;end select
