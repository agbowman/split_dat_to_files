 DECLARE drmmi_get_mock_id(dgmi_env_id=f8) = f8
 DECLARE drmmi_backfill_mock_id(dbmi_env_id=f8) = f8
 SUBROUTINE drmmi_set_mock_id(dsmi_cur_id,dsmi_final_tgt_id,dsmi_mock_ind)
   DECLARE dsmi_info_char = vc WITH protect, noconstant("")
   DECLARE dsmi_mock_str = vc WITH protect, noconstant("")
   SET dsmi_info_char = drdc_to_string(dsmi_cur_id)
   SET dm_err->eproc = "Delete current mock setting."
   DELETE  FROM dm_info di
    WHERE di.info_domain="DATA MANAGEMENT"
     AND di.info_name IN ("RDDS_MOCK_ENV_ID", "RDDS_NO_MOCK_ENV_ID")
    WITH nocounter
   ;end delete
   IF (check_error(dm_err->eproc)=1)
    ROLLBACK
    CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
    RETURN(- (1))
   ENDIF
   IF (dsmi_mock_ind=1)
    SET dsmi_mock_str = "RDDS_MOCK_ENV_ID"
   ELSE
    SET dsmi_mock_str = "RDDS_NO_MOCK_ENV_ID"
   ENDIF
   SET dm_err->eproc = "Inserting new mock setting into dm_info."
   INSERT  FROM dm_info di
    SET di.info_domain = "DATA MANAGEMENT", di.info_name = dsmi_mock_str, di.info_number =
     dsmi_final_tgt_id,
     di.info_char = dsmi_info_char, di.updt_applctx = reqinfo->updt_applctx, di.updt_dt_tm =
     cnvtdatetime(curdate,curtime3),
     di.updt_cnt = 0, di.updt_id = reqinfo->updt_id, di.updt_task = reqinfo->updt_task
    WITH nocounter
   ;end insert
   IF (check_error(dm_err->eproc)=1)
    ROLLBACK
    CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
    RETURN(- (1))
   ENDIF
   SET dm_err->eproc = "Log Mock Copy of Prod Change event."
   SET stat = initrec(auto_ver_request)
   SET stat = initrec(auto_ver_reply)
   SET stat = alterlist(auto_ver_request->qual,1)
   SET auto_ver_request->qual[1].rdds_event = "Mock Copy of Prod Change"
   SET auto_ver_request->qual[1].cur_environment_id = dsmi_cur_id
   SET auto_ver_request->qual[1].paired_environment_id = dsmi_final_tgt_id
   EXECUTE dm_rmc_auto_verify_setup
   IF ((auto_ver_reply->status="F"))
    ROLLBACK
    SET dm_err->err_ind = 1
    SET dm_err->emsg = auto_ver_reply->status_msg
    CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
    RETURN(- (1))
   ELSE
    COMMIT
   ENDIF
   RETURN(1)
 END ;Subroutine
 SUBROUTINE drmmi_get_mock_id(dgmi_env_id)
   DECLARE dgmi_mock_id = f8 WITH protect, noconstant(0.0)
   DECLARE dgmi_info_char = vc WITH protect, noconstant("")
   IF (dgmi_env_id=0.0)
    SET dm_err->eproc = "Gathering environment_id from dm_info."
    SELECT INTO "nl:"
     FROM dm_info di
     WHERE di.info_name="DM_ENV_ID"
      AND di.info_domain="DATA MANAGEMENT"
     DETAIL
      dgmi_env_id = di.info_number
     WITH nocounter
    ;end select
    IF (check_error(dm_err->eproc)=1)
     CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
     RETURN(- (1))
    ELSEIF (dgmi_env_id=0.0)
     SET dm_err->err_ind = 1
     SET dm_err->emsg = "Could not retrieve valid environment_id"
     CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
     RETURN(- (1))
    ENDIF
   ENDIF
   SET dgmi_info_char = drdc_to_string(dgmi_env_id)
   SET dm_err->eproc = "Querying dm_info for mock id."
   SELECT INTO "nl:"
    FROM dm_info di
    WHERE di.info_domain="DATA MANAGEMENT"
     AND di.info_name IN ("RDDS_MOCK_ENV_ID", "RDDS_NO_MOCK_ENV_ID")
     AND di.info_char=dgmi_info_char
    DETAIL
     IF (di.info_name="RDDS_MOCK_ENV_ID")
      dgmi_mock_id = di.info_number
     ELSE
      dgmi_mock_id = dgmi_env_id
     ENDIF
    WITH nocounter
   ;end select
   IF (check_error(dm_err->eproc)=1)
    CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
    RETURN(- (1))
   ELSEIF (curqual > 1)
    SET dm_err->err_ind = 1
    SET dm_err->emsg = "Invalid MOCK setup detected."
    CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
    RETURN(- (1))
   ENDIF
   IF (dgmi_mock_id=0.0)
    SET dgmi_mock_id = drmmi_backfill_mock_id(dgmi_env_id)
    IF (dgmi_mock_id < 0.0)
     RETURN(- (1))
    ENDIF
   ENDIF
   RETURN(dgmi_mock_id)
 END ;Subroutine
 SUBROUTINE drmmi_backfill_mock_id(dbmi_env_id)
   DECLARE dbmi_mock_id = f8 WITH protect, noconstant(0.0)
   DECLARE dbmi_info_char = vc WITH protect, noconstant("")
   DECLARE dbmi_continue = i2 WITH protect, noconstant(0)
   SET dbmi_info_char = drdc_to_string(dbmi_env_id)
   WHILE (dbmi_continue=0)
     SET drl_reply->status = ""
     SET drl_reply->status_msg = ""
     CALL get_lock("RDDS MOCK ID BACKFILL",dbmi_info_char,0,drl_reply)
     IF ((drl_reply->status="F"))
      CALL disp_msg(drl_reply->status_msg,dm_err->logfile,1)
      SET dm_err->err_ind = 1
      SET dm_err->emsg = drl_reply->status_msg
      CALL remove_lock("RDDS MOCK ID BACKFILL",dbmi_info_char,currdbhandle,drl_reply)
      RETURN(- (1))
     ELSEIF ((drl_reply->status="Z"))
      CALL pause(10)
     ELSE
      SET dbmi_continue = 1
     ENDIF
   ENDWHILE
   SET dm_err->eproc = "Querying dm_info for mock id."
   SELECT INTO "nl:"
    FROM dm_info di
    WHERE di.info_domain="DATA MANAGEMENT"
     AND di.info_name IN ("RDDS_MOCK_ENV_ID", "RDDS_NO_MOCK_ENV_ID")
     AND di.info_char=dbmi_info_char
    DETAIL
     IF (di.info_name="RDDS_MOCK_ENV_ID")
      dbmi_mock_id = di.info_number
     ELSE
      dbmi_mock_id = dbmi_env_id
     ENDIF
    WITH nocounter
   ;end select
   IF (check_error(dm_err->eproc)=1)
    CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
    CALL remove_lock("RDDS MOCK ID BACKFILL",dbmi_info_char,currdbhandle,drl_reply)
    RETURN(- (1))
   ENDIF
   IF (dbmi_mock_id=0.0)
    UPDATE  FROM dm_info di
     SET di.info_char = dbmi_info_char
     WHERE di.info_domain="DATA MANAGEMENT"
      AND di.info_name="RDDS_MOCK_ENV_ID"
     WITH nocounter
    ;end update
    IF (check_error(dm_err->eproc)=1)
     CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
     ROLLBACK
     CALL remove_lock("RDDS MOCK ID BACKFILL",dbmi_info_char,currdbhandle,drl_reply)
     RETURN(- (1))
    ELSE
     COMMIT
    ENDIF
    IF (curqual=0)
     SET dm_err->eproc = "Updating RDDS_NO_MOCK_ENV_ID row."
     UPDATE  FROM dm_info di
      SET di.info_number = 0.0, di.info_char = dbmi_info_char, di.updt_applctx = reqinfo->
       updt_applctx,
       di.updt_dt_tm = cnvtdatetime(curdate,curtime3), di.updt_cnt = 0, di.updt_id = reqinfo->updt_id,
       di.updt_task = reqinfo->updt_task
      WHERE di.info_domain="DATA MANAGEMENT"
       AND di.info_name="RDDS_NO_MOCK_ENV_ID"
      WITH nocounter
     ;end update
     IF (check_error(dm_err->eproc)=1)
      CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
      ROLLBACK
      CALL remove_lock("RDDS MOCK ID BACKFILL",dbmi_info_char,currdbhandle,drl_reply)
      RETURN(- (1))
     ELSE
      COMMIT
     ENDIF
     IF (curqual=0)
      SET dm_err->eproc = "Inserting RDDS_NO_MOCK_ENV_ID row."
      INSERT  FROM dm_info di
       SET di.info_domain = "DATA MANAGEMENT", di.info_name = "RDDS_NO_MOCK_ENV_ID", di.info_number
         = 0.0,
        di.info_char = dbmi_info_char, di.updt_applctx = reqinfo->updt_applctx, di.updt_dt_tm =
        cnvtdatetime(curdate,curtime3),
        di.updt_cnt = 0, di.updt_id = reqinfo->updt_id, di.updt_task = reqinfo->updt_task
       WITH nocounter
      ;end insert
      IF (check_error(dm_err->eproc)=1)
       CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
       ROLLBACK
       CALL remove_lock("RDDS MOCK ID BACKFILL",dbmi_info_char,currdbhandle,drl_reply)
       RETURN(- (1))
      ELSE
       COMMIT
      ENDIF
     ENDIF
     SET dbmi_mock_id = dbmi_env_id
    ELSE
     SET dm_err->eproc = "Querying for mock id."
     SELECT INTO "nl:"
      FROM dm_info di
      WHERE di.info_domain="DATA MANAGEMENT"
       AND di.info_name="RDDS_MOCK_ENV_ID"
       AND di.info_char=dbmi_info_char
      DETAIL
       dbmi_mock_id = di.info_number
      WITH nocounter
     ;end select
     IF (check_error(dm_err->eproc)=1)
      CALL disp_msg(dm_err->emsg,dm_err->logfile,dm_err->err_ind)
      CALL remove_lock("RDDS MOCK ID BACKFILL",dbmi_info_char,currdbhandle,drl_reply)
      RETURN(- (1))
     ENDIF
    ENDIF
   ENDIF
   CALL remove_lock("RDDS MOCK ID BACKFILL",dbmi_info_char,currdbhandle,drl_reply)
   RETURN(dbmi_mock_id)
 END ;Subroutine
 DECLARE v_env_id = f8
 DECLARE v_env_name = vc
 DECLARE v_rows = i4
 DECLARE xcnt = i4
 DECLARE begin_cnt = i4
 DECLARE end_cnt = i4
 DECLARE where_str = vc
 DECLARE dbixc_parm = i2
 DECLARE v_merge_link = vc
 DECLARE v_source_env_id = f8
 DECLARE v_dmt_table_name = vc
 DECLARE v_env_mock_id = f8
 DECLARE dxb_check_progress(s_src_env_id=f8,s_tgt_env_id=f8,s_pl_name=vc) = i2
 FREE RECORD rec_backfill_data
 RECORD rec_backfill_data(
   1 rec_table_cnt = i4
   1 rec_table_info[*]
     2 table_name = vc
     2 pk_column = vc
     2 r_table_name = vc
     2 r_tab_exist_ind = i2
 )
 FREE RECORD rec_xlats
 RECORD rec_xlats(
   1 xlat_cnt = i4
   1 xlats[*]
     2 to_value = f8
 )
 IF (validate(reply->status,"1")="1"
  AND validate(reply->status,"2")="2")
  FREE RECORD reply
  RECORD reply(
    1 status = c1
      2 status_msg = vc
  )
 ENDIF
 SET v_rows = 0
 SET where_str = ""
 SET dbixc_parm =  $1
 SET v_source_env_id =  $2
 SET dm_err->eproc = "Getting local environment info"
 CALL disp_msg(" ",dm_err->logfile,0)
 SELECT INTO "nl:"
  FROM dm_info a,
   dm_environment b
  PLAN (a
   WHERE a.info_name="DM_ENV_ID"
    AND a.info_domain="DATA MANAGEMENT")
   JOIN (b
   WHERE a.info_number=b.environment_id)
  DETAIL
   v_env_id = b.environment_id, v_env_name = b.environment_name
  WITH nocounter
 ;end select
 IF (curqual=0)
  SET dm_err->eproc = "Fatal Error: current environment id not found"
  CALL disp_msg(" ",dm_err->logfile,0)
  GO TO exit_program_child
 ENDIF
 SET v_env_mock_id = drmmi_get_mock_id(v_env_id)
 IF (v_env_mock_id < 0)
  CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
  GO TO exit_program_child
 ENDIF
 IF (dbixc_parm=0)
  SET v_dmt_table_name = "DM_MERGE_TRANSLATE"
 ELSEIF (dbixc_parm != 0)
  SET v_merge_link = concat("@MERGE",trim(cnvtstring(v_source_env_id)),trim(cnvtstring(v_env_id)))
  SET v_dmt_table_name = concat("DM_MERGE_TRANSLATE",v_merge_link)
  IF (dxb_check_progress(v_source_env_id,v_env_mock_id,v_merge_link)=0)
   SET dm_err->err_ind = 1
   GO TO exit_program_child
  ENDIF
  IF ((reply->status="Z"))
   SET dm_err->eproc = reply->status_msg
   GO TO exit_program_child
  ELSEIF ((reply->status="S"))
   SET dm_err->eproc = reply->status_msg
   GO TO exit_program_child
  ENDIF
 ENDIF
 SET stat = alterlist(rec_backfill_data->rec_table_info,20)
 SET dm_err->eproc = "Getting tables that have translations"
 CALL disp_msg(" ",dm_err->logfile,0)
 SELECT
  IF (dbixc_parm=0)
   FROM dm_columns_doc dcd,
    user_tab_columns utc
   WHERE dcd.table_name IN (
   (SELECT DISTINCT
    table_name
    FROM (parser(v_dmt_table_name) dmt)
    WHERE dmt.env_target_id=v_env_mock_id))
    AND dcd.root_entity_name=dcd.table_name
    AND dcd.root_entity_attr=dcd.column_name
    AND utc.table_name=dcd.table_name
    AND utc.column_name=dcd.column_name
  ELSEIF (dbixc_parm != 0)
   FROM dm_columns_doc dcd,
    user_tab_columns utc
   WHERE dcd.table_name IN (
   (SELECT DISTINCT
    table_name
    FROM (parser(v_dmt_table_name) dmt)
    WHERE dmt.env_target_id=v_source_env_id
     AND dmt.env_source_id=v_env_mock_id))
    AND dcd.root_entity_name=dcd.table_name
    AND dcd.root_entity_attr=dcd.column_name
    AND utc.table_name=dcd.table_name
    AND utc.column_name=dcd.column_name
  ELSE
  ENDIF
  INTO "nl:"
  dcd.table_name
  DETAIL
   v_rows = (v_rows+ 1)
   IF (mod(v_rows,20)=1
    AND v_rows != 1)
    stat = alterlist(rec_backfill_data->rec_table_info,(v_rows+ 19))
   ENDIF
   rec_backfill_data->rec_table_info[v_rows].table_name = dcd.table_name, rec_backfill_data->
   rec_table_info[v_rows].pk_column = dcd.column_name
  FOOT REPORT
   stat = alterlist(rec_backfill_data->rec_table_info,v_rows), rec_backfill_data->rec_table_cnt =
   v_rows
  WITH nocounter
 ;end select
 IF (check_error(dm_err->eproc)=1)
  CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
  GO TO exit_program_child
 ENDIF
 IF ((rec_backfill_data->rec_table_cnt > 0))
  SET dm_err->eproc = "Finding table suffixes for $R table_names"
  CALL disp_msg(" ",dm_err->logfile,0)
  SELECT INTO "nl:"
   dtd.table_suffix
   FROM dm_tables_doc dtd,
    (dummyt d  WITH seq = rec_backfill_data->rec_table_cnt)
   PLAN (d)
    JOIN (dtd
    WHERE (dtd.table_name=rec_backfill_data->rec_table_info[d.seq].table_name))
   DETAIL
    rec_backfill_data->rec_table_info[d.seq].r_table_name = cutover_tab_name(rec_backfill_data->
     rec_table_info[d.seq].table_name,dtd.table_suffix)
   WITH nocounter
  ;end select
  IF (check_error(dm_err->eproc)=1)
   CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
   GO TO exit_program_child
  ENDIF
  SET dm_err->eproc = "Querying user_tables to determine if $R table exists"
  CALL disp_msg(" ",dm_err->logfile,0)
  SELECT INTO "nl:"
   FROM user_tables ut,
    (dummyt d  WITH seq = rec_backfill_data->rec_table_cnt)
   PLAN (d)
    JOIN (ut
    WHERE (ut.table_name=rec_backfill_data->rec_table_info[d.seq].r_table_name))
   DETAIL
    rec_backfill_data->rec_table_info[d.seq].r_tab_exist_ind = 1
   WITH nocounter
  ;end select
  IF (check_error(dm_err->eproc)=1)
   CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
   GO TO exit_program_child
  ENDIF
 ENDIF
 SET dm_err->eproc = "Finding invalid translations"
 CALL disp_msg(" ",dm_err->logfile,0)
 FOR (v_rows = 1 TO rec_backfill_data->rec_table_cnt)
   IF ( NOT ((rec_backfill_data->rec_table_info[v_rows].table_name IN ("DCP_FORMS_REF",
   "DCP_SECTION_REF", "APPLICATION_TASK"))))
    SELECT INTO "NL:"
     FROM user_tables u
     WHERE (u.table_name=rec_backfill_data->rec_table_info[v_rows].table_name)
     WITH nocounter
    ;end select
    IF (check_error(dm_err->eproc)=1)
     CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
     GO TO exit_program_child
    ENDIF
    IF (curqual > 0)
     SET stat = alterlist(rec_xlats->xlats,0)
     SET rec_xlats->xlat_cnt = 0
     IF (dbixc_parm=0)
      SET where_str = concat(" NOT EXISTS (SELECT 'X' FROM ",rec_backfill_data->rec_table_info[v_rows
       ].table_name," where ",rec_backfill_data->rec_table_info[v_rows].pk_column," = dmt.to_value)")
      IF ((rec_backfill_data->rec_table_info[v_rows].r_tab_exist_ind=1))
       SET where_str = concat(where_str,"and NOT EXISTS (SELECT 'X' FROM ",rec_backfill_data->
        rec_table_info[v_rows].r_table_name," where ",rec_backfill_data->rec_table_info[v_rows].
        pk_column,
        " = dmt.to_value and RDDS_STATUS_FLAG<9000 and RDDS_DELETE_IND = 0)")
      ENDIF
      SET dm_err->eproc = "Selecting from dm_merge_translate locally"
      CALL disp_msg(" ",dm_err->logfile,0)
      SELECT DISTINCT INTO "nl:"
       dmt.to_value
       FROM dm_merge_translate dmt
       WHERE dmt.env_target_id=v_env_mock_id
        AND (dmt.table_name=rec_backfill_data->rec_table_info[v_rows].table_name)
        AND  NOT ( EXISTS (
       (SELECT
        "x"
        FROM dm_refchg_invalid_xlat
        WHERE (parent_entity_name=rec_backfill_data->rec_table_info[v_rows].table_name)
         AND dmt.to_value=parent_entity_id)))
        AND parser(where_str)
       HEAD REPORT
        rec_xlats->xlat_cnt = 0
       DETAIL
        rec_xlats->xlat_cnt = (rec_xlats->xlat_cnt+ 1)
        IF (mod(rec_xlats->xlat_cnt,20)=1)
         stat = alterlist(rec_xlats->xlats,(rec_xlats->xlat_cnt+ 19))
        ENDIF
        rec_xlats->xlats[rec_xlats->xlat_cnt].to_value = dmt.to_value
       FOOT REPORT
        stat = alterlist(rec_xlats->xlats,rec_xlats->xlat_cnt)
       WITH nocounter
      ;end select
      IF (check_error(dm_err->eproc)=1)
       CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
       GO TO exit_program_child
      ENDIF
     ELSE
      SET where_str = concat(" NOT EXISTS (SELECT 'X' FROM ",rec_backfill_data->rec_table_info[v_rows
       ].table_name," where ",rec_backfill_data->rec_table_info[v_rows].pk_column,
       " = dmt.from_value)")
      SET dm_err->eproc = "Selecting from dm_merge_translate remotely"
      CALL disp_msg(" ",dm_err->logfile,0)
      SELECT INTO "nl:"
       dmt.from_value
       FROM (parser(v_dmt_table_name) dmt)
       WHERE dmt.env_target_id=v_source_env_id
        AND dmt.env_source_id=v_env_mock_id
        AND (dmt.table_name=rec_backfill_data->rec_table_info[v_rows].table_name)
        AND  NOT ( EXISTS (
       (SELECT
        "x"
        FROM dm_refchg_invalid_xlat
        WHERE (parent_entity_name=rec_backfill_data->rec_table_info[v_rows].table_name)
         AND dmt.from_value=parent_entity_id)))
        AND parser(where_str)
       HEAD REPORT
        rec_xlats->xlat_cnt = 0
       DETAIL
        rec_xlats->xlat_cnt = (rec_xlats->xlat_cnt+ 1)
        IF (mod(rec_xlats->xlat_cnt,20)=1)
         stat = alterlist(rec_xlats->xlats,(rec_xlats->xlat_cnt+ 19))
        ENDIF
        rec_xlats->xlats[rec_xlats->xlat_cnt].to_value = dmt.from_value
       FOOT REPORT
        stat = alterlist(rec_xlats->xlats,rec_xlats->xlat_cnt)
       WITH nocounter
      ;end select
      IF (check_error(dm_err->eproc)=1)
       CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
       GO TO exit_program_child
      ENDIF
     ENDIF
     IF ((rec_xlats->xlat_cnt > 0))
      FOR (xcnt = 1 TO ((rec_xlats->xlat_cnt/ 1000)+ 1))
        SET begin_cnt = (((xcnt - 1) * 1000)+ 1)
        SET end_cnt = (xcnt * 1000)
        IF ((end_cnt > rec_xlats->xlat_cnt))
         SET end_cnt = rec_xlats->xlat_cnt
        ENDIF
        SET dm_err->eproc = "Inserting a batch of 1000 rows into dm_refchg_invalid_xlat"
        CALL disp_msg(" ",dm_err->logfile,0)
        INSERT  FROM dm_refchg_invalid_xlat drix,
          (dummyt d  WITH seq = rec_xlats->xlat_cnt)
         SET drix.parent_entity_id = rec_xlats->xlats[d.seq].to_value, drix.parent_entity_name =
          cnvtupper(rec_backfill_data->rec_table_info[v_rows].table_name), drix.updt_id = 0,
          drix.updt_dt_tm = cnvtdatetime(curdate,curtime3), drix.updt_task = 0, drix.updt_applctx = 0,
          drix.updt_cnt = 0
         PLAN (d
          WHERE d.seq >= begin_cnt
           AND d.seq <= end_cnt)
          JOIN (drix)
         WITH nocounter
        ;end insert
        IF (check_error(dm_err->eproc)=1)
         CALL disp_msg(dm_err->emsg,dm_err->logfile,1)
         ROLLBACK
         GO TO exit_program_child
        ENDIF
        COMMIT
      ENDFOR
     ENDIF
    ENDIF
   ENDIF
 ENDFOR
 IF (dbixc_parm != 0)
  INSERT  FROM dm_info di
   SET di.info_domain = "INVALID DMT XLAT BACKFILL COMPLETE", di.info_name = v_merge_link, di
    .info_number = cnvtint(currdbhandle),
    di.updt_dt_tm = cnvtdatetime(curdate,curtime3)
   WITH nocounter
  ;end insert
  IF (check_error(dm_err->eproc)=1)
   ROLLBACK
   GO TO exit_program_child
  ENDIF
  DELETE  FROM dm_info di
   WHERE di.info_domain="INVALID DMT XLAT BACKFILL IN PROGRESS"
    AND di.info_name=v_merge_link
   WITH nocounter
  ;end delete
  IF (check_error(dm_err->eproc)=1)
   ROLLBACK
   GO TO exit_program_child
  ENDIF
  SET reply->status = "S"
  SET reply->status_msg = "Backfill process completed. Starting merge process"
  COMMIT
 ENDIF
 SUBROUTINE dxb_check_progress(s_src_env_id,s_tgt_env_id,s_pl_name)
   DECLARE s_rdb_handle = f8
   SET dm_err->eproc = "Checking to see if the backfill process is complete."
   CALL disp_msg(" ",dm_err->logfile,0)
   SELECT INTO "NL:"
    FROM dm_info di
    WHERE di.info_domain="INVALID DMT XLAT BACKFILL COMPLETE"
     AND di.info_name=s_pl_name
    WITH nocounter
   ;end select
   IF (check_error(dm_err->eproc)=1)
    RETURN(0)
   ENDIF
   IF (curqual != 0)
    RETURN(1)
    SET reply->status = "S"
    SET reply->status_msg = "Backfill was successful. Starting merge process."
   ENDIF
   SET dm_err->eproc = "Checking to see if the backfill process is in progress."
   CALL disp_msg(" ",dm_err->logfile,0)
   SELECT INTO "NL:"
    di.info_number
    FROM dm_info di
    WHERE di.info_domain="INVALID DMT XLAT BACKFILL IN PROGRESS"
     AND di.info_name=s_pl_name
    DETAIL
     s_rdb_handle = di.info_number
    WITH nocounter, forupdatewait(di)
   ;end select
   IF (check_error(dm_err->eproc)=1)
    RETURN(0)
   ENDIF
   IF (curqual > 0)
    SELECT INTO "NL:"
     FROM gv$session g
     WHERE g.audsid=s_rdb_handle
     WITH nocounter
    ;end select
    IF (check_error(dm_err->eproc)=1)
     ROLLBACK
     RETURN(0)
    ENDIF
    IF (curqual=0)
     DELETE  FROM dm_info di
      WHERE di.info_domain="INVALID DMT XLAT BACKFILL IN PROGRESS"
       AND di.info_name=s_pl_name
      WITH nocounter
     ;end delete
     IF (check_error(dm_err->eproc)=1)
      ROLLBACK
      RETURN(0)
     ENDIF
    ELSE
     ROLLBACK
     SET reply->status = "Z"
     SET reply->status_msg = "Backfill in process. Pausing 60 seconds before checking again"
     RETURN(1)
    ENDIF
   ENDIF
   INSERT  FROM dm_info di
    SET di.info_domain = "INVALID DMT XLAT BACKFILL IN PROGRESS", di.info_name = s_pl_name, di
     .info_number = cnvtint(currdbhandle),
     di.updt_dt_tm = cnvtdatetime(curdate,curtime3)
    WITH nocounter
   ;end insert
   IF (check_error(dm_err->eproc)=1)
    ROLLBACK
    RETURN(0)
   ENDIF
   COMMIT
   SET reply->status = "P"
   SET reply->status_msg = "Begin backfill process"
   RETURN(1)
 END ;Subroutine
#exit_program_child
 IF ((dm_err->err_ind=1))
  SET reply->status = "F"
  SET reply->status_msg = dm_err->emsg
 ENDIF
END GO
